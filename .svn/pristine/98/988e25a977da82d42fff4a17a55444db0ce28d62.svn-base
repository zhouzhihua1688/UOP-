                    <div class="tabbable">
                        <ul class="nav nav-tabs padding-12 tab-color-blue background-blue">
                            <li class="active">
                                <a data-toggle="tab" href="#eventPage">事件</a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#businessPage">业务节点</a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#nodeTypePage">节点类型</a>
                            </li>
                            <li>
                                <a data-toggle="tab" href="#paramsPage">参数</a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div id="eventPage" class="tab-pane in active">
                                <div class="form-inline" style="padding: 10px 0;">
                                    <!--<div class="form-group">-->
                                    <!--<label class="control-label">custNo:</label>-->
                                    <!--<input type="text" v-model.trim="custNo">-->
                                    <!--</div>-->

                                    <div class="form-group">
                                        <!-- <label class="control-label">模型ID:</label> -->
