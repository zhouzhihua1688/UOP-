                            <div class="modal-content">
                                <div class="modal-header" style="padding: 10px;">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                    <h4 class="modal-title" v-text="page1.titleStatus">新增信息</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="form " style="padding: 0 50px 0 24px;">
                                        <div class="form-group" >
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                事件名称:</label>
                                            <input type="text" style="width: 60%;" v-model.trim="diaeventName" placeholder="请输入事件名称"/>
                                        </div>
                                        <div class="form-group" >
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                事件key:</label>
                                            <input type="text" style="width: 60%;" v-model.trim="diaeventKey" placeholder="请输入事件名称"/>
                                        </div>
                                        <div class="form-group" >
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                事件触发范围过滤方法:</label>
                                            <input type="text" style="width: 60%;" v-model.trim="diaeventFilterMethod" placeholder="请输入"/>
                                        </div>
                                        <div class="form-group" >
                                            <label class="control-label" style="width: 150px;text-align: right;vertical-align: top;">
                                                事件描述:</label>
                                            <textarea name="" id="" cols="30" rows="10"  style="width: 60%;" placeholder="请输入事件描述" v-model.trim="diaeventDesc"></textarea>    
                                            <!-- <input type="text" style="width: 60%;" v-model.trim="diatargetTypeDesc" /> -->
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label" style="width: 150px;text-align: right;vertical-align: top;">
                                                事件参数:</label>
                                                <select multiple="" id="state1" name="state1" class="select2" data-placeholder="请选择事件参数...">
                                                    <option v-for="(item,index) in page4.SpareData" :value="item.pkey" v-text="item.pname" :key="index"></option>
                                                </select>   
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label" style="width: 150px;text-align: right;vertical-align: top;">
                                                业务节点:</label>
                                                <select multiple="" id="state2" name="state2" class="select2" data-placeholder="请选择业务节点...">
                                                    <option v-for="(item,index) in page2.SpareData" :value="item.id" v-text="item.nodeName" :key="index"></option>
                                                </select>   
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label" style="width: 150px;text-align: right;vertical-align: top;">
                                                条件表达式生成方式:</label>
                                                <select id="state3" name="state3" v-model="diaexpGenerateType">
                                                    <option value="1">自选生成</option>
                                                    <option value="2">配置模板生成</option>
                                                </select>   
                                        </div>
                                        <div class="form-group" v-show="diaexpGenerateType=='2'">
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                表达式配置模板:</label>
                                            <select id="" name="" v-model="diaexpConfigTemplate">
                                                <option  v-for="(item,index) in templateList" :key="index" :value="item.value" v-text="item.key"></option>
                                            </select>       
                                        </div>
                                        <div class="form-group" >
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                触发数据类型:</label>
                                            <select id="" name="" v-model="diatriggerDataTp">
                                                <option  v-for="(key,value,index) in dataTp" :key="index" :value="value" v-text="key"></option>
                                            </select>       
                                        </div>
                                        <div class="form-group" v-show="diatriggerDataTp=='3'">
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                触发数据外调来源:</label>
                                            <select id="" name="" v-model="diatriggerDataFrom">
                                                <option  v-for="(key,value,index) in dataFrom" :key="index" :value="value" v-text="key"></option>
                                            </select>       
                                        </div>
                                    </div>
                                </div>    
                                <p class="text-center">
                                    <button class="btn btn-sm btn-primary" @click="add1">保存</button>
                                    <button class="btn btn-default btn-sm" data-dismiss="modal">取消</button>
                                </p>
                            </div>
                        </div>
                    </div>
                    <!-- 删除1 -->
