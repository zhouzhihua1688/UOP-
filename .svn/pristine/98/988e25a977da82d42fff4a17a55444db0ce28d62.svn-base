                                            查询
                                        </button>
                                        <% if(canShow||authorityList.includes('C')) { %>
                                        <button  class="btn btn-sm btn-primary" value="" @click="showAdd1"
                                            >
                                            <i class="ace-icon glyphicon glyphicon-plus"></i>
                                            新增事件
                                        </button>
                                        <% } %>
                                    </div>
                                </div>

                                <div class="table-header">
                                    事件表格数据
                                </div>
                                <!--分页器和快速查询-->
                                <div class="row" style="padding:8px 0;background-color: #EFF3F8; margin: 0;">
                                    <div class="col-xs-6">
                                        <div class="dataTables_length" id="dynamic-table_length">
                                            <label>每页显示</label>
                                            <select class="input-sm" name="pageMaxNum1" v-model.trim="pageMaxNum1">
                                                <option value="10">10</option>
                                                <option value="50">20</option>
                                                <option value="100">50</option>
                                                <option value="500">100</option>
                                            </select>
                                            <label>条</label>
                                        </div>
                                    </div>
                                </div>
                                <!--数据表-->
                                <table id="simple-table" class="table table-bordered table-hover dataTable">
                                    <thead>
                                        <tr>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">事件名称</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">业务节点</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">参数</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">创建人</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改人</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">创建时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">操作</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <template v-if="page1.tableData.length>0">
                                            <tr v-for="(item,index) in page1.tableData" :key="index">
                                               <td class="center" v-text="item.eventName"></td>
                                               <td class="center" v-text="item.nodeIds"></td>
                                               <td class="center" v-text="item.eventConditionParams"></td>
                                               <td class="center" v-text="item.createBy"></td>
                                               <td class="center" v-text="item.modifyBy"></td>
                                               <td class="center" v-text="item.createTime"></td>
                                               <td class="center" v-text="item.updateTime"></td>
                                                <td class="center">
                                                    <% if(canShow||authorityList.includes('U')) { %>
                                                    <a href="javascript:;" @click="showUpdate1(item)">编辑</a>
                                                    <% } %>
                                                    <% if(canShow||authorityList.includes('D')) { %>
                                                    &nbsp;
                                                    <a href="javascript:;" class="red" @click="deleteData1(item)">删除</a>
                                                    <% } %>
                                                </td>
                                            </tr>
                                        </template>
                                        <template v-else>
                                            <tr>
                                                <td class="center" v-text="page1.loadingStatus" colspan="8"></td>
                                            </tr>
                                        </template>
                                    </tbody>
                                </table>
                                <!--底部页码-->
                                <div v-if="page1.tableData.length>0" class="row"
                                    style="border-bottom: 1px solid #e0e0e0;padding-top: 12px;padding-bottom: 12px;background-color: #EFF3F8;margin: 0;">
                                    <div class="col-xs-2">
                                        <div class="dataTables_info" v-text="'共 '+page1.totalPage+' 页'"></div>
                                    </div>
                                    <div class="col-xs-10">
                                        <div class="dataTables_paginate paging_simple_numbers"
                                            id="dynamic-table_paginate">
                                            <ul class="pagination">
                                                <li class="paginate_button"
                                                    v-show="pageList.call(page1).length<page1.totalPage&&page1.currentIndex>page1.maxSpace">
                                                    <a href="javascript:;" v-text="'1...'" @click="toFirst.call(page1)"></a>
                                                </li>
                                                <li :class="page1.currentIndex==0?'paginate_button previous disabled':'paginate_button previous'"
                                                    @click="prev.call(page1)">
                                                    <a href="javascript:;" v-text="'上一页'"></a>
                                                </li>
                                                <li :class="value==page1.currentIndex+1?'paginate_button active':'paginate_button'"
                                                    v-for="value in pageList.call(page1)" @click="changeIndex.call(page1,value)">
                                                    <a href="javascript:;" v-text="value"></a>
                                                </li>
                                                <li class="paginate_button"
                                                    v-show="pageList.call(page1).length<page1.totalPage&&page1.totalPage-page1.currentIndex>page1.maxSpace">
                                                    <a href="javascript:;" v-text="'...'+page1.totalPage" @click="toLast.call(page1)"></a>
                                                </li>
                                                <li :class="page1.currentIndex==page1.totalPage-1?'paginate_button next disabled':'paginate_button next'"
                                                    @click="next.call(page1)">
                                                    <a href="javascript:;" v-text="'下一页'"></a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
