                            <th class="center">开放日期</th>
                            <th class="center">类型</th>
                            <th class="center">交易账号</th>
                            <th class="center">申请用户名</th>
                            <th class="center">申请额度</th>
                            <th class="center">实配额度</th>
                            <th class="center">实际使用额度</th>
                            <th class="center">流水号</th>
                            <th class="center">申请日期</th>
                            <th class="center">发送状态</th>
                            <th class="center">发送日期</th>
                            <th class="center">失效日期</th>
                            <th class="center">受理方式</th>
                            <th class="center">手机号</th>
                            <!--<th class="center">客户经理</th>-->
                            <!--<th class="center">历史资产峰值</th>-->
                            <!--<th class="center">当前现金宝资产市值</th>-->
                            <th class="center">使用情况</th>
                            <th class="center">预约码状态</th>
                            <th class="center">操作</th>
                        </tr>
                        </thead>
                        <tbody>
                        <!--真分页业务数据-->
                        <template v-if="tableData.length>0">
                            <tr v-for="(item,index) in tableData" :key="index"
                                :style="index%2==0?'background-color: #f9f9f9;':''">
                                <td class="center">
                                    <label class="pos-rel">
                                        <input type="checkbox" class="ace" v-model="item.check" @click="check(index)"/>
                                        <span class="lbl"></span>
                                    </label>
                                </td>
                                <td class="center" v-text="item.fundId"></td>
                                <td class="center" v-text="item.fundName"></td>
                                <td class="center" v-text="item.nextOpenDate"></td>
                                <td class="center">{{item.type | type}}</td>
                                <td class="center" v-text="item.tradeAcco"></td>
                                <td class="center" v-text="item.custNm"></td>
                                <td class="center" v-text="item.reserveAmt"></td>
                                <td class="center" v-text="item.realAmt"></td>
                                <td class="center" v-text="item.subAmt"></td>
                                <td class="center" v-text="item.reserveNo"></td>

                                <td class="center" v-text="item.reserveDate"></td>
                                <td class="center">
                                    {{item.transType | transType}}
                                </td>
                                <td class="center" v-text="item.transDate"></td>
                                <td class="center" v-text="item.deadlineDate"></td>
                                <td class="center">
                                    {{item.accptMd | accptMd}}
                                </td>
                                <td v-text="item.mobileNo"></td>
                                <td class="center">
                                    {{item.status | status}}
                                </td>
                                <td class="center">
                                    {{item.auditStatus | auditStatus}}
                                </td>
                                <td class="center">                                   
                                    <template v-if="item.transType=='Y'">
                                        <span href="javascript:;" v-text="'发送成功'"></span>
                                    </template>
                                    <template v-else>
                                        <a href="javascript:;" @click="showUpdate(item)">修改</a>
                                    </template>
                                </td>
                            </tr>
                        </template>
                        <template v-else>
                            <tr>
                                <td class="center" v-text="'没有数据'" colspan="22"></td>
                            </tr>
                        </template>
                        </tbody>
                    </table>

                    <!--底部页码-->
                    <!--后端真分页-->
                    <div v-if="tableData.length>0" class="row"
                         style="border-bottom: 1px solid #e0e0e0;padding-top: 12px;padding-bottom: 12px;background-color: #EFF3F8;margin: 0;">
                        <div class="col-xs-2">
                            <div class="dataTables_info" v-text="'共 '+totalPage+' 页'"></div>
                        </div>
                        <div class="col-xs-10">
                            <div class="dataTables_paginate paging_simple_numbers" id="dynamic-table_paginate">
                                <ul class="pagination">
                                    <li class="paginate_button"
                                        v-show="pageList.length<totalPage&&currentIndex>maxSpace">
                                        <a href="javascript:;" v-text="'1...'" @click="toFirst"></a>
                                    </li>
                                    <li :class="currentIndex==0?'paginate_button previous disabled':'paginate_button previous'"
                                        @click="prev">
                                        <a href="javascript:;" v-text="'上一页'"></a>
                                    </li>
                                    <li :class="value==currentIndex+1?'paginate_button active':'paginate_button'"
                                        v-for="value in pageList" @click="changeIndex(value)">
                                        <a href="javascript:;" v-text="value"></a>
                                    </li>
                                    <li class="paginate_button"
                                        v-show="pageList.length<totalPage&&totalPage-currentIndex>maxSpace">
                                        <a href="javascript:;" v-text="'...'+totalPage" @click="toLast"></a>
                                    </li>
                                    <li :class="currentIndex==totalPage-1?'paginate_button next disabled':'paginate_button next'"
                                        @click="next">
                                        <a href="javascript:;" v-text="'下一页'"></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- /.page-content -->
        </div>
    </div><!-- /.main-content -->
    <% include ../../common/footer.html %>
</div><!-- /.main-container -->
<% include ../../common/basic-scripts.html %>
