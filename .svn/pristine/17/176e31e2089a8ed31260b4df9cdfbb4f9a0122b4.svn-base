<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../../common/head.html %>
    <!-- inline styles related to this page -->
    <link rel="stylesheet" href="/ace/assets/css/select2.min.css" />
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-datetimepicker.min.css"/>
    <style>
        .table > tbody > tr > td {
            vertical-align: middle;
            word-break: break-all;
        }
        .colorBlock{
            display: inline-block;
            width: 60px;
            height: 30px;
            vertical-align: middle;
            margin-right: 10px;
            cursor: pointer;
        }
        .chosenActive{
            box-shadow: 0 0 10px #f00;
        }
    </style>
</head>
<body class="no-skin">
<% include ../../common/header.html %>
<div class="main-container ace-save-state" id="main-container">
    <% include ../../common/sidebar.html %>
    <% include ../../common/left-menu.html %>
    <div class="main-content">
        <div class="main-content-inner">
            <div class="page-content" id="content">
                <% include ../../common/breadcrumb.html %>
                <% include ../../common/ace-settings-container.html %>
                <!-- loading动画 -->
                <div class="cover" v-show="loadingShow" style="z-index: 1999;display: none;">
                    <img src="/img/loading.gif" alt="上传中" style="position: absolute;top: 50%;left: 50%;">
                </div>
                <!-- loading动画end -->
                <!--第一级新增/修改弹窗-->
                <div class="modal" id="add" tabindex="-1" role="dialog" aria-hidden="true"
                     data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                    &times;
                                </button>
                                <h4 class="modal-title" style="display: inline">新增内容:</h4>
                            </div>
                            <div class="modal-body">
                                <div class="form " style="padding: 0 50px 0 24px;">
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题名称:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diathemenm"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题标题:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diathemeTitle"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">主题标题展示模式 :</label>
                                        <select name="" id="" v-model="diathemeTitleDisplayMode">
                                            <option value="">请选择</option>
                                            <option value="titleDefault">默认(titleDefault)</option>
                                            <option value="titleWithLeftFlag">主题带左浮标(titleWithLeftFlag)</option>
                                            <option value="titleVipGradient">尊享渐变(titleVipGradient)</option>
                                            <option value="topRoundRect">顶部圆角样式(topRoundRect)</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题副标:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diathemesubTitle"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right;vertical-align: top;">主题描述:</label>
                                        <textarea name="" id="" style="width: 60%;" v-model.trim="diathemeDescription"></textarea>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            模块左上标题:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diaupperleftTitle"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            模块显示更多标题:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diaviewmoreTitle"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            显示更多url:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diaviewmoreUrl"
                                        />
                                    </div>
                                    <div class="form-group" style="display: flex;">
                                        <label class="control-label" style="width: 150px;text-align: right">查看更多图片地址:</label>
                                        <input type="text" id="uploadInput5" placeholder="请选择"
                                               style="width: 37%;" disabled/>
                                        <input type="button" class="btn btn-sm btn-primary" id="uploadBtn5" value="选择" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-primary" @click="uploadPic5" value="上传" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-danger" @click="rmImgUrl" value="删除">
                                        <input id="uploadFileInput5" type="file" name="file" style="display: none"
                                               class="upload"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            模块内容显示数量<span class="red">*</span>:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diadisplayCount"
                                        />
                                    </div>
                                    <div class="form-group" style="display: flex;">
                                        <label class="control-label" style="width: 150px;text-align: right">主题图标图片地址(上传):</label>
                                        <input type="text" id="uploadInput1" placeholder="请选择"
                                               style="width: 37%;" disabled/>
                                        <input type="button" class="btn btn-sm btn-primary" id="uploadBtn1" value="选择" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-primary" @click="uploadPic1" value="上传" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-danger" @click="rmImgUrl" value="删除">
                                        <input id="uploadFileInput1" type="file" name="file" style="display: none"
                                               class="upload"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题底部按钮名称:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diabottomBtnName"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题底部按钮跳转url:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diabottomBtnUrl"
                                        />
                                    </div>
                                    <div class="form-group" style="display: flex;">
                                        <label class="control-label" style="width: 150px;text-align: right">图片地址:</label>
                                        <input type="text" id="uploadInput2" placeholder="请选择"
                                               style="width: 37%;" disabled/>
                                        <input type="button" class="btn btn-sm btn-primary" id="uploadBtn2" value="选择" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-primary" @click="uploadPic2" value="上传" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-danger" @click="rmImgUrl" value="删除">
                                        <input id="uploadFileInput2" type="file" name="file" style="display: none"
                                               class="upload"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">展示类型:</label>
                                        <select name="" id="" v-model="diashowtp">
                                            <option value="2">展示该主题下的配置的内容数据</option>
                                            <option value="1">展示该主题</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            模块与主题关系排序<span class="red">*</span>:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diasort"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            每行显示项数<span class="red">*</span>:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diacolumnNum"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题展示行数:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diarowNum"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">主题对应内容类型:</label>
                                        <input type="text" style="width: 60%;" disabled v-model="diacontentName"/>
                                        <!-- <select name="" id="" v-model="diacontentTp" >
                                            <option v-for="item in contentTpList" :value="item.contentKey" v-text="item.contentName"></option>
                                        </select> -->
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题对应模版类型:</label>
                                        <input type="text" style="width: 60%;" disabled v-model="diatemId"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">数据来源:</label>
                                        <select name="" id="" v-model="diadataFrom">
                                            <option value="0">配置数据</option>
                                            <option value="1">外调获取</option>
                                            <option value="2">智能推荐</option>
                                            <option value="3">自动化运营-配置数据</option>
                                            <option value="4">配置数据-基于目标因子获取</option>
                                            <option value="5">配置+文件读取</option>
                                        </select>
                                    </div>
                                    <div class="form-group" v-show="diadataFrom==1||diadataFrom==2||diadataFrom==5">
                                        <label class="control-label" style="width: 150px;text-align: right">数据场景key:</label>
                                        <select name="" id="" v-model="diadynamicscenekey">
                                            <option v-for="item in scenekeyList" :value="item.sceneKey" v-text="item.sceneName"></option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">主题标题位置:</label>
                                        <select name="" id="" v-model="diathemeTitlePosition">
                                            <option value="">请选择</option>
                                            <option value="1">左</option>
                                            <option value="2">中</option>
                                            <option value="3">右</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">主题背景模式:</label>
                                        <select name="" id="" v-model="diabackgroundMode">
                                            <option value="">请选择</option>
                                            <option value="taskGridCard">任务网格卡片(taskGridCard)</option>
                                            <option value="gridDivider">网格分割线(gridDivider)</option>
                                            <option value="roundRect">圆角矩形(roundRect)</option>
                                            <option value="topRoundRect">顶部圆角矩形(topRoundRect)</option>
                                            <option value="bottomRoundRect">底部圆角样式(bottomRoundRect)</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">是否需要上边距:</label>
                                        <select name="" id="" v-model="diahasTopMargin">
                                            <option value="1">需要</option>
                                            <option value="0">不需要</option>
                                        </select>
                                    </div>
                                    <div class="form-group" style="display: flex;">
                                        <label class="control-label" style="width: 150px;text-align: right">主题标题图片地址:</label>
                                        <input type="text" id="uploadInput3" placeholder="请选择"
                                               style="width: 37%;" disabled/>
                                        <input type="button" class="btn btn-sm btn-primary" id="uploadBtn3" value="选择" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-primary" @click="uploadPic3" value="上传" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-danger" @click="rmImgUrl" value="删除">
                                        <input id="uploadFileInput3" type="file" name="file" style="display: none"
                                               class="upload"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">主题背景色:</label>
                                        <span class="colorBlock" style="background: #fbf3e4;" data-color="#fbf3e4" ></span>
                                        <span class="colorBlock" style="background: #e9ebf7;" data-color="#e9ebf7" ></span>
                                        <span class="colorBlock" style="background: #fbecec;" data-color="#fbecec" ></span>
                                        <span class="colorBlock" style="background: #e7f1f9;" data-color="#e7f1f9" ></span>
                                    </div>
                                    <!-- <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题背景图片:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diabackgroundImageUrl"
                                        />
                                    </div> -->
                                    <div class="form-group" style="display: flex;">
                                        <label class="control-label" style="width: 150px;text-align: right">主题背景图片:</label>
                                        <input type="text" id="uploadInput6" placeholder="请选择"
                                               style="width: 37%;" disabled/>
                                        <input type="button" class="btn btn-sm btn-primary" id="uploadBtn6" value="选择" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-primary" @click="uploadPic6" value="上传" style="margin-right:1px">
                                        <input type="button" class="btn btn-sm btn-danger" @click="rmImgUrl" value="删除">
                                        <input id="uploadFileInput6" type="file" name="file" style="display: none"
                                               class="upload"/>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">
                                            主题外部导航栏颜色:</label>
                                        <input type="text" style="width: 60%;" v-model.trim="diaexternalNavigationColor"
                                        />
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right">是否禁用主题背景模式:</label>
                                        <select name="" id="" v-model="diadisableBackgroundMode">
                                            <option value="0">开启</option>
                                            <option value="1">禁用</option>
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label" style="width: 150px;text-align: right;vertical-align: top;">备注<span class="red">*</span>:</label>
                                        <textarea name="" id="" style="width: 60%;" v-model.trim="diaremark" placeholder="必填，神策统计使用"></textarea>
                                    </div>

                                </div>
                            </div>
                            <p class="text-center">
                                <button class="btn btn-sm btn-primary" @click="add">保存</button>
                                <button class="btn btn-default btn-sm" data-dismiss="modal">取消</button>
                            </p>
                        </div>
                    </div>
                </div>
                <!--信息提示弹窗-->
                <div class="modal" id="info" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                <h4 class="modal-title">提示信息</h4>
                            </div>
                            <div class="modal-body text-center">
                                <h4 v-text="diaMsg"></h4>
                            </div>
                            <p class="text-center">
                                <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">确定</button>
                            </p>
                        </div>
                    </div>
                </div>
                <!--查看主题内容弹窗-->
                <div class="modal" id="checkThemeContent" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog" style="width: 80%;">
                        <div class="modal-content" >
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                <h4 class="modal-title">内容配置列表</h4>
                            </div>
                            <div class="modal-body text-center">
                                <div class="form-inline" style="padding: 10px 0;">
                                    <div class="form-group">
                                        <label class="control-label">主题对应内容类型:</label>
                                        <span v-text="positionName+'('+contentTp+')'"></span>
                                    </div>
                                    <div class="form-group pull-right" style="margin-bottom: 10px">
                                        <input type="button" class="btn btn-sm btn-primary" value="新建内容" @click="showAddSecond">
                                    </div>
                                </div>
                                <!--数据表-->
                                <template v-if="contentTp=='index'">
                                    <table id="index" class="table table-bordered table-hover dataTable">
                                        <thead>
                                        <tr>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">热力指数模板ID</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">指数编码</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">指数名称</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">排序</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1" v-show="isDataForm">目标过滤因子</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">开始时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">结束时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改人</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">操作</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <template v-if="themeContentData.length>0">
                                            <tr v-for="(item,index) in themeContentData" :key="index"
                                            :style="currentTime>item.endTime?'background-color: #EEE;':''">
                                                <td class="center" v-text='item.objconfigId'></td>
                                                <td class="center" v-text='item.object.indexCode'></td>
                                                <td class="center" v-text='item.object.indexName'></td>
                                                <td class="center" v-text='item.position'></td>
                                                <td class="center" v-show="isDataForm" v-text='item.targetobjectkey?item.targetobjectkey:"--"'></td>
                                                <td class="center" v-text='formatTime(item.startTime)'></td>
                                                <td class="center" v-text='formatTime(item.endTime)'></td>
                                                <td class="center" v-text='formatTime(item.modifyTime)'></td>
                                                <td class="center" v-text='item.modifyBy'></td>
                                                <td class="center" >
                                                    <a href="javascript:;" @click="showUpdateSecond(item)" >修改</a>/<a href="javascript:;" @click="deleteParamsSecond(item)" >删除</a>/<a href="javascript:;" @click="goToPageUpdate(item)">内容详情</a>/
                                                    <a href="javascript:;" @click="goToPageTiming(item)">定时任务</a>
                                                    
                                                </td>
                                            </tr>
                                        </template>
                                        <template v-else>
                                            <tr>
                                                <template v-if="isDataForm">
                                                    <td class="center"  colspan="10">暂无数据</td>
                                                </template>
                                                <template v-else>
                                                    <td class="center"  colspan="9">暂无数据</td>
                                                </template>
                                            </tr>
                                        </template>
                                        </tbody>
                                    </table>
                                </template>
                                <template v-if="contentTp=='wx_product'">
                                    <table id="wx_product" class="table table-bordered table-hover dataTable">
                                        <thead>
                                        <tr>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">产品模板ID</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">产品标题</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">排序</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1" v-show="isDataForm">目标过滤因子</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">开始时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">结束时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改人</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">操作</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <template v-if="themeContentData.length>0">
                                            <tr v-for="(item,index) in themeContentData" :key="index"
                                            :style="currentTime>item.endTime?'background-color: #EEE;':''">
                                                <td class="center" v-text='item.objconfigId'></td>
                                                <td class="center" v-text='item.object.prdTitle'></td>
                                                <td class="center" v-text='item.position'></td>
                                                <td class="center" v-show="isDataForm" v-text='item.targetobjectkey?item.targetobjectkey:"--"'></td>
                                                <td class="center" v-text='formatTime(item.startTime)'></td>
                                                <td class="center" v-text='formatTime(item.endTime)'></td>
                                                <td class="center" v-text='formatTime(item.modifyTime)'></td>
                                                <td class="center" v-text='item.modifyBy'></td>
                                                <td class="center" >
                                                    <a href="javascript:;" @click="showUpdateSecond(item)" >修改</a>/<a href="javascript:;" @click="deleteParamsSecond(item)" >删除</a>/<a href="javascript:;" @click="goToPageUpdate(item)">内容详情</a>/
                                                    <a href="javascript:;" @click="goToPageTiming(item)">定时任务</a>
                                                </td>
                                            </tr>
                                        </template>
                                        <template v-else>
                                            <tr>
                                                <template v-if="isDataForm">
                                                    <td class="center"  colspan="9">暂无数据</td>
                                                </template>
                                                <template v-else>
                                                    <td class="center"  colspan="8">暂无数据</td>
                                                </template>
                                            </tr>
                                        </template>
                                        </tbody>
                                    </table>
                                </template>
                                <template v-if="contentTp=='manager'">
                                    <table id="manager" class="table table-bordered table-hover dataTable">
                                        <thead>
                                        <tr>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">基金经理名称</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">排序</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1" v-show="isDataForm">目标过滤因子</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">开始时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">结束时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改时间</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">修改人</th>
                                            <th class="center col-sm-1 col-md-1 col-lg-1">操作</th>
                                        </tr>
                                        </thead>
