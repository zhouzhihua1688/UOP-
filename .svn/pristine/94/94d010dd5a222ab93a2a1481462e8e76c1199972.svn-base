                                        <div class="form-horizontal">
                                            <div class="form-group">
                                                <label class="col-sm-7 control-label">
                                                    指标值：{{calcValue}}</label>
                                            </div>
                                        </div>
                                        <p class="text-center">
                                            <button class="btn btn-info btn-sm" type="button" @click='calc'>
                                                实时试算
                                            </button>
                                            <button type="button" class="btn btn-info btn-sm" @click='createIndex'
                                                style="margin-left: 60px;">
                                                生成指标
                                            </button>
                                        </p>
                                    </div>
                                </div>
                            </div>
                            <!-- DIALOG ENDS -->

                            <div class="form-inline">
                                <div class="form-group " @click ="labelQuery('ParentPlatform')">
                                    <label style="width: 80px;">一级平台：</label>
                                    <select-chosen1 :list='parentPlatformList' v-model='label.parentPlatform' ></select-chosen1>
                                </div>
                                <div class="form-group " @click="labelQuery('SalePlatform')">
                                    <label style="width: 80px;">二级平台：</label>
                                    <select-chosen1 :list='platformList' v-model='label.salePlatform' ></select-chosen1>
                                </div>
                                <div class="form-group " @click="labelQuery('SalePosition')">
                                    <label style="width: 80px;">专区：</label>
                                    <select-chosen1 :list='positionList' v-model='label.salePosition' ></select-chosen1>
                                </div>
                                <div class="form-group " @click="labelQuery('InvestArea')">
                                    <label style="width: 80px;">赛道：</label>
                                    <select-chosen1 :list='investAreaList' v-model='label.investArea' ></select-chosen1>
                                </div>
                                <div class="form-group " @click="labelQuery('ProductId')" >
                                    <label style="width: 80px;">产品：</label>
                                    <select-chosen1 :list='productList' v-model='label.productId' ></select-chosen1>
                                </div>
                                <!-- <div class="form-group ">
                                    <input type="button" class="btn btn-sm btn-primary " value="查询" @click="queryData">
                                </div> -->
                                <div class="form-group pull-right">
                                    <input type="button" class="btn btn-sm btn-primary " value="添加"
                                        @click="showSettingDialog">
                                </div>
                                <div class="form-group pull-right" style="margin-right:30px;">
                                    <input type="button" class="btn btn-sm btn-primary " @click='getTableData(0)'
                                        value="查询">
                                </div>
                            </div>
                            <div class="table-header">
                                <%= cpage %>--表格数据
                            </div>
                            <!--分页器和快速查询-->
                            <div class="row" style="padding:8px 0;background-color: #EFF3F8; margin: 0;">
                                <div class="col-xs-6">
                                    <div class="dataTables_length" id="dynamic-table_length">
                                        <label>每页显示</label>
                                        <select class="input-sm" name="pageMaxNum" v-model.trim="pageMaxNum">
                                            <option value="10">10</option>
                                            <option value="50">50</option>
                                            <option value="100">100</option>
                                            <option value="500">500</option>
                                        </select>
                                        <label>条</label>
                                    </div>
                                </div>
                            </div>
                            <!--数据表-->
                            <div style="width: 100%;">
                                <table id="simple-table" class="table table-bordered table-hover dataTable">
                                    <thead>
                                        <tr>
                                            <th class="center ">一级平台</th>
                                            <th class="center ">二级平台</th>
                                            <th class="center ">专区</th>
                                            <th class="center ">赛道</th>
                                            <th class="center ">产品代码</th>
                                            <th class="center ">产品名称</th>
                                            <th class="center ">监控指标类型</th>
                                            <th class="center ">监控指标名称</th>
                                            <th class="center ">监控值</th>
                                            <th class="center ">预警值</th>
                                            <th class="center ">创建人</th>
                                            <th class="center ">状态</th>
                                            <th class="center ">操作</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <template v-if="tableData.length>0">
                                            <tr v-for="(item,index) in tableData" :key="index">
                                                <td class="center" v-text='item.parentPlatform'></td>
                                                <td class="center" v-text='item.salePlatform'></td>
                                                <td class="center" v-text='item.salePosition'></td>
                                                <td class="center" v-text='item.investArea'></td>
                                                <td class="center" v-text='item.productid'></td>
                                                <td class="center" v-text='item.productName'></td>
                                                <td class="center">{{item.showCategory | showCategoryText}}</td>
                                                <td class="center" v-text='item.indexName'></td>
                                                <td class="center" v-text='item.targetValue'></td>
                                                <td class="center" v-text='item.alarmValue'></td>
                                                <td class="center" v-text='item.author'></td>
                                                <td class="center">{{item.calcStatus==='M'?'指标计算中':'完成'}}</td>
                                                <td class="center">
                                                    <a href="javascript:;" @click='showDetail(item)'>设置</a>
                                                    /
                                                    <a href="javascript:;" class='red'
                                                        @click='showDeleteDialog(item)'>删除</a>
                                                </td>
                                            </tr>
                                        </template>
                                        <template v-else>
                                            <tr>
                                                <td class="center" v-text="'没有数据'" colspan="13"></td>
                                            </tr>
                                        </template>
                                    </tbody>
                                </table>
                            </div>

                            <!--底部页码-->
                            <div v-if="tableData.length>0" class="row"
                                style="border-bottom: 1px solid #e0e0e0;padding-top: 12px;padding-bottom: 12px;background-color: #EFF3F8;margin: 0;">
                                <div class="col-xs-2">
                                    <div class="dataTables_info" v-text="'共 '+totalPage+' 页'"></div>
                                </div>
                                <div class="col-xs-10">
                                    <div class="dataTables_paginate paging_simple_numbers" id="dynamic-table_paginate">
                                        <ul class="pagination">
                                            <li class="paginate_button"
                                                v-show="pageList.length<totalPage&&currentIndex>maxSpace">
                                                <a href="javascript:;" v-text="'1...'" @click="toFirst"></a>
                                            </li>
                                            <li :class="currentIndex==0?'paginate_button previous disabled':'paginate_button previous'"
                                                @click="prev">
                                                <a href="javascript:;" v-text="'上一页'"></a>
                                            </li>
                                            <li :class="value==currentIndex+1?'paginate_button active':'paginate_button'"
                                                v-for="value in pageList" @click="changeIndex(value)">
                                                <a href="javascript:;" v-text="value"></a>
                                            </li>
                                            <li class="paginate_button"
                                                v-show="pageList.length<totalPage&&totalPage-currentIndex>maxSpace">
                                                <a href="javascript:;" v-text="'...'+totalPage" @click="toLast"></a>
                                            </li>
                                            <li :class="currentIndex==totalPage-1?'paginate_button next disabled':'paginate_button next'"
                                                @click="next">
                                                <a href="javascript:;" v-text="'下一页'"></a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div><!-- /.row -->
                </div><!-- /.page-content -->
            </div>
        </div><!-- /.main-content -->
        <% include ../../common/footer.html %>
    </div><!-- /.main-container -->
    <% include ../../common/basic-scripts.html %>
    <!-- 开发环境版本，包含了用帮助的命令行警告 -->
    <script src="/js/lib/vue.js"></script>
    <script src="/ace/assets/js/select2.min.js"></script>
    <script src="/ace/assets/js/chosen.jquery.min.js"></script>
    <script src="/js/productIndexes/monitoring/indicator.js"></script>
</body>

</html>