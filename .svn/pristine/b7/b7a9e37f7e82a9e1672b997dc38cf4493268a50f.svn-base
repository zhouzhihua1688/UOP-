<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../../common/head.html %>
    <!-- inline styles related to this page -->
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-datetimepicker.min.css"/>
    <style>
        .table > tbody > tr > td {
            vertical-align: middle;
        }
        .cover {
            position: fixed;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(9, 10, 10, 0.527);
            z-index: 999;
        }
    </style>
</head>
<body class="no-skin">
<% include ../../common/header.html %>
<div class="main-container ace-save-state" id="main-container">
    <% include ../../common/sidebar.html %>
    <% include ../../common/left-menu.html %>
    <div class="main-content">
        <div class="main-content-inner">
            <div class="page-content" id="content">
                <% include ../../common/breadcrumb.html %>
                <% include ../../common/ace-settings-container.html %>
                <!-- loading动画 -->
                <div class="cover" v-show="loadingShow" style="z-index: 1999;display: none;">
                    <img src="/img/loading.gif" alt="上传中" style="position: absolute;top: 50%;left: 50%;">
                </div><!-- loading动画 -->
                <!--信息提示弹窗-->
                <div class="modal" id="info" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                <h4 class="modal-title">提示信息</h4>
                            </div>
                            <div class="modal-body text-center">
                                <h4 v-text="diaMsg"></h4>
                            </div>
                            <p class="text-center">
                                <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">确定</button>
                            </p>
                        </div>
                    </div>
                </div>
                <!--删除弹窗-->
                <div class="modal" id="delete" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                <h4 class="modal-title">提示信息</h4>
                            </div>
                            <p>
                            <div class="modal-body text-center">
                                <h4>是否确认删除?</h4>
                            </div>
                            </p>
                            <p class="text-center">
                                <button type="button" class="btn btn-primary btn-sm" @click="deleteData">确定</button>
                                <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">取消</button>
                            </p>
                        </div>
                    </div>
                </div>
                <!--新增弹窗-->
                <div class="modal" id="add" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;
                                </button>
                                <h4 class="modal-title inline">模板操作</h4>
                            </div>
                            <div class="modal-body">
                                <div class="form" style="padding: 0 12px;">
                                    <div class="form-group">
                                        <label class="control-label">
                                            <label class="control-label" style="width: 70px;">模板名称:</label>
                                            <input type="text" placeholder="必填" v-model="diaTemplateName">
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">
                                            <label class="control-label" style="width: 70px;">消息渠道:</label>
                                            <select class="form-control" style="width: 170px;display: inline"
                                                    v-model="diaChannelType" :disabled="!!updateId">
                                                <option value="1">App站内信</option>
                                                <option value="2">App-push</option>
                                                <option value="3">短信</option>
                                                <option value="4">微信</option>
                                                <option value="5">微信高端</option>
                                            </select>
                                        </label>
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==1">
                                        <label class="control-label" style="width: 70px;">模板摘要:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaTemplateExplain">
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==1">
                                        <label class="control-label" style="width: 70px;">标题:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填" v-model="diaTitle">
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==4">
                                        <label class="control-label" style="width: 70px;">模板选择:</label>
                                        <select class="form-control" style="width: 170px;display: inline"
                                                v-model="diaWXType">
                                            <option value="servive">服务状态通知</option>
                                            <option value="trade">交易通知</option>
                                            <option value="monthBill">月度对账单提醒</option>
                                            <option value="investment">投顾服务通知</option>
                                        </select>
                                    </div>
                                    <!--微信模板: 服务状态通知-->
                                    <div class="form-group" v-show="isWXtype1">
                                        <label class="control-label" style="width: 70px;">欢迎语:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData1.text1">
                                    </div>
                                    <div class="form-group" v-show="isWXtype1">
                                        <label class="control-label" style="width: 70px;">服务名称:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData1.text2">
                                    </div>
                                    <div class="form-group" v-show="isWXtype1">
                                        <label class="control-label" style="width: 70px;">服务状态:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData1.text3">
                                    </div>
                                    <div class="form-group" v-show="isWXtype1">
                                        <label class="control-label" style="width: 70px;">问题详情:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData1.text4">
                                        <span @click="showWXContent"><a href="javascript:;">预览</a></span>
                                    </div>
                                    <div class="form-group" v-show="isWXtype1">
                                        <label class="control-label" style="width: 70px;">备注:</label>
                                        <textarea cols="58" rows="3" style="vertical-align:top;"
                                                  v-model="diaWXData1.text5"></textarea>
                                    </div>
                                    <!--微信模板: 交易通知-->
                                    <div class="form-group" v-show="isWXtype2">
                                        <label class="control-label" style="width: 70px;">欢迎语:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData2.text1">
                                    </div>
                                    <div class="form-group" v-show="isWXtype2">
                                        <label class="control-label" style="width: 70px;">业务:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData2.text2">
                                    </div>
                                    <div class="form-group" v-show="isWXtype2">
                                        <label class="control-label" style="width: 70px;">金额:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData2.text3">
                                    </div>
                                    <div class="form-group" v-show="isWXtype2">
                                        <label class="control-label" style="width: 70px;">状态:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData2.text4">
                                    </div>
                                    <div class="form-group" v-show="isWXtype2">
                                        <label class="control-label" style="width: 70px;">时间:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData2.text5">
                                        <span @click="showWXContent"><a href="javascript:;">预览</a></span>
                                    </div>
                                    <div class="form-group" v-show="isWXtype2">
                                        <label class="control-label" style="width: 70px;">备注:</label>
                                        <textarea cols="58" rows="3" style="vertical-align:top;"
                                                  v-model="diaWXData2.text6"></textarea>
                                    </div>
                                    <!--微信模板: 月度对账单提醒-->
                                    <div class="form-group" v-show="isWXtype3">
                                        <label class="control-label" style="width: 70px;">欢迎语:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData3.text1">
                                    </div>
                                    <div class="form-group" v-show="isWXtype3">
                                        <label class="control-label" style="width: 75px;">对账单日期:</label>
                                        <input type="text" style="width: 79%;" placeholder="必填"
                                               v-model="diaWXData3.text2">
                                        <span @click="showWXContent"><a href="javascript:;">预览</a></span>
                                    </div>
                                    <div class="form-group" v-show="isWXtype3">
                                        <label class="control-label" style="width: 70px;">备注:</label>
                                        <textarea cols="58" rows="3" style="vertical-align:top;"
                                                  v-model="diaWXData3.text3"></textarea>
                                    </div>
                                    <!--微信模板: 投顾服务通知-->
                                    <div class="form-group" v-show="isWXtype4">
                                        <label class="control-label" style="width: 70px;">欢迎语:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData4.text1">
                                    </div>
                                    <div class="form-group" v-show="isWXtype4">
                                        <label class="control-label" style="width: 70px;">服务名称:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData4.text2">
                                    </div>
                                    <div class="form-group" v-show="isWXtype4">
                                        <label class="control-label" style="width: 70px;">服务时间:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXData4.text3">
                                        <span @click="showWXContent"><a href="javascript:;">预览</a></span>
                                    </div>
                                    <div class="form-group" v-show="isWXtype4">
                                        <label class="control-label" style="width: 70px;">备注:</label>
                                        <textarea cols="58" rows="3" style="vertical-align:top;"
                                                  v-model="diaWXData4.text4"></textarea>
                                    </div>
                                    <!--微信高端模板-->
                                    <div class="form-group" v-show="diaChannelType==5">
                                        <label class="control-label" style="width: 70px;">欢迎语:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXHighData.text1">
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==5">
                                        <label class="control-label" style="width: 70px;">业务:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXHighData.text2">
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==5">
                                        <label class="control-label" style="width: 70px;">金额:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXHighData.text3">
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==5">
                                        <label class="control-label" style="width: 70px;">状态:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXHighData.text4">
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==5">
                                        <label class="control-label" style="width: 70px;">时间:</label>
                                        <input type="text" style="width: 80%;" placeholder="必填"
                                               v-model="diaWXHighData.text5">
                                        <span @click="showWXContent"><a href="javascript:;">预览</a></span>
                                    </div>
                                    <div class="form-group" v-show="diaChannelType==5">
                                        <label class="control-label" style="width: 70px;">备注:</label>
                                        <textarea cols="58" rows="3" style="vertical-align:top;"
                                                  v-model="diaWXHighData.text6"></textarea>
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label" style="margin-right: -55px;">
                                            <label class="control-label" style="width: 70px;">模板内容:</label>
                                            <textarea :disabled="diaChannelType==4||diaChannelType==5" cols="58"
                                                      rows="6"
                                                      style="vertical-align:top;" v-model="diaContent"></textarea>
                                        </label>
                                    </div>
                                    <div class="form-group" v-show="jumpPageShow">
                                        <label class="control-label" style="width: 70px;">跳转页面:</label>
                                        <select class="form-control" style="width: 170px;display: inline"
                                                v-model="diaLinkType">
                                            <option v-for="(item,index) in jumpLinkList" :value="item.name"
                                                    v-text="item.name"></option>
                                            <option v-if="diaChannelType==2&&!updateId" value="后端配置">后端配置</option>
                                            <option v-if="(diaChannelType==1||diaChannelType==2)&&!updateId" value="空">
                                                空
                                            </option>
                                        </select>
                                        <span class="red"
                                              v-show="needShowPreUrl">已加前缀:htffundxjb://action?type=url&link=</span>
                                    </div>
                                    <div class="form-group" v-if="hasfundId">
                                        <label class="control-label" style="width: 70px;">基金ID:</label>
                                        <input type="text" style="width: 80%;" v-model="diafundId">
                                    </div>
                                    <div class="form-group" v-if="jumpLinkShow">
                                        <label class="control-label" style="width: 70px;">链接地址:</label>
                                        <template v-if="!needInputUrl&&diaChannelType!=4">
                                            <input type="text" disabled style="width: 80%;" v-model="showDiaUrl">
                                        </template>
                                        <template v-else>
                                            <input type="text" :placeholder="diaChannelType==2?'必填':''"
                                                   style="width: 80%;" v-model="diaUrl">
                                        </template>
                                    </div>
                                    <div class="form-group" style="position: relative;" v-show="diaChannelType==1">
                                        <div class="input-group">
                                            <label class="control-label" style="width: 70px;">开始时间:</label>
                                            <input type="text" id="startTime" class="date-timepicker"
                                                   style="width: 145px;">
                                            <span class="input-group-addon" style="width: 16%;">
                                                <i class="fa fa-clock-o bigger-110"></i>
                                            </span>
                                        </div>
                                        <div class="input-group" style="position: absolute;top: 0;right: 0;">
                                            <label class="control-label" style="width: 70px;">结束时间:</label>
                                            <input type="text" id="endTime" class="date-timepicker"
                                                   style="width: 145px;">
                                            <span class="input-group-addon" style="width: 16%;">
                                                <i class="fa fa-clock-o bigger-110"></i>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="form-group" style="position: relative;" v-show="diaChannelType==1">
                                        <div class="input-group">
                                            <label class="control-label" for="showTimeInterval" style="width: 100px;">显示时间间隔:</label>
                                            <input type="checkbox" class="ace" id="showTimeInterval"
                                                   v-model="showTimeInterval" @click="checkTimeInterval"/>
                                            <span class="lbl"></span>
                                        </div>
                                    </div>
                                    <div class="form-group" style="position: relative;"
                                         v-show="diaChannelType==1&&showTimeInterval==true">
                                        <div class="input-group">
                                            <label class="control-label" style="width: 70px;">时间间隔:</label>
                                            <input type="number" class="ace" v-model="timeInterval"
                                                   oninput="this.value=this.value.replace(/[^0-9.]/g,'')"/>
                                            <span class="lbl"></span>
                                        </div>
                                    </div>
                                </div>
                                <p class="text-center">
                                    <button class="btn btn-sm btn-primary" @click="add">保存</button>
                                    <button class="btn btn-sm btn-primary" data-dismiss="modal" aria-hidden="true">取消
                                    </button>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <!--批量新增弹窗-->
                <div class="modal" id="addExcel" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;
                                </button>
                                <h4 class="modal-title inline">批量新增模板</h4>
                            </div>
                            <div class="modal-body">
                                <div class="form" style="padding: 0 12px;">
                                    <div class="form-group" style="margin-top: 20px;">
                                        <span class="control-label" style="margin-right: 20px;">数据表:</span>
                                        <input type="text" id="uploadInput" style="width: 50%;"
                                               placeholder="请上传.xlsx文件" disabled/>
                                        <input type="button" class="btn btn-sm btn-primary" id="uploadBtn" value="选择">
                                        <input type="button" class="btn btn-sm btn-danger" @click="downloadFile"
                                               value="下载模板文件">
                                    </div>
                                </div>
                                <input id="uploadFileInput" type="file" style="display: none"/>
                                <p class="text-center">
                                    <button class="btn btn-sm btn-primary" @click="addExcel">新增</button>
                                    <button class="btn btn-sm btn-primary" data-dismiss="modal" aria-hidden="true">取消
                                    </button>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <!--批量修改弹窗-->
                <div class="modal" id="updateExcel" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;
                                </button>
                                <h4 class="modal-title inline">批量修改模板</h4>
                            </div>
                            <div class="modal-body">
                                <div class="form" style="padding: 0 12px;">
                                    <div class="form-group" style="margin-top: 20px;">
                                        <span class="control-label" style="margin-right: 20px;">数据表:</span>
                                        <input type="text" id="updateInput" style="width: 50%;"
                                               placeholder="请上传.xlsx文件" disabled/>
                                        <input type="button" class="btn btn-sm btn-primary" id="updateBtn" value="选择">
                                        <input type="button" class="btn btn-sm btn-danger" @click="downloadUpdateFile"
                                               value="下载模板文件">
                                    </div>
                                </div>
                                <input id="updateFileInput" type="file" style="display: none"/>
                                <p class="text-center">
                                    <button class="btn btn-sm btn-primary" @click="updateExcel">修改</button>
                                    <button class="btn btn-sm btn-primary" data-dismiss="modal" aria-hidden="true">取消
                                    </button>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="form-inline" style="padding: 10px 0;">
                    <div class="form-group">
                        <label class="control-label">模板ID:</label>
                        <input type="text" v-model="templateId">
                    </div>
                    <div class="form-group">
                        <label class="control-label">模板名称:</label>
                        <input type="text" v-model="templateName">
                    </div>
                    <div class="form-group">
                        <label class="control-label">创建人:</label>
                        <input type="text" v-model="creator">
                    </div>
                    <div class="form-group">
                        <label class="control-label">消息渠道:</label>
                        <select class="form-control" style="width: 170px;display: inline" v-model="channelType">
                            <option value=""></option>
                            <option value="1">App站内信</option>
                            <option value="2">App-push</option>
                            <option value="3">短信</option>
                            <option value="4">微信</option>
                            <option value="5">微信高端</option>
                        </select>
                    </div>
                    <div class="form-group pull-right">
                        <input type="button" class="btn btn-sm btn-primary" value="查询" @click="search">
                        <input type="button" class="btn btn-sm btn-primary" value="新增" @click="showAdd">
                        <input type="button" class="btn btn-sm btn-primary" value="批量新增" @click="showAddExcel">
                        <input type="button" class="btn btn-sm btn-primary" value="批量修改" @click="showUpdateExcel">
                        <input type="button" class="btn btn-sm btn-danger " value="全量导出" @click="exportAll">
                    </div>
                </div>

                <div class="table-header">
                    <%= cpage %>--表格数据
                </div>

                <!--分页器和快速查询-->
                <div class="row" style="padding:8px 0;background-color: #EFF3F8; margin: 0;">
                    <div class="col-xs-6">
                        <div class="dataTables_length">
                            <label>每页显示</label>
                            <select class="input-sm" name="pageMaxNum" v-model="pageMaxNum">
                                <option value="10">10</option>
                                <option value="20">20</option>
                                <option value="50">50</option>
                            </select>
                            <label>条</label>
                        </div>
                    </div>
                    <div class="col-xs-6">
                        <div id="dynamic-table_filter" class="dataTables_filter">
                            <label>快速查询:</label>
                            <input type="text" class="input-sm" v-model="condition">
                        </div>
                    </div>
                </div>
                <!--数据表-->
                <table id="simple-table" class="table table-striped table-bordered table-hover dataTable"
                       style="margin-bottom: 0;">
                    <thead>
                    <tr>
                        <th class="center">模板ID</th>
                        <th class="center">模板名称</th>
                        <th class="center">模板摘要</th>
                        <th class="center">创建时间</th>
                        <th class="center">消息渠道</th>
                        <th class="center">结束时间</th>
                        <th class="center">创建人</th>
                        <th class="center">操作</th>
                    </tr>
                    </thead>
                    <tbody>
                    <template v-if="viewData.length>0">
                        <tr v-for="(item,index) in viewData" :key="index">
                            <td class="center col-sm-2" v-text="item.templateId"></td>
                            <td class="center col-sm-1" v-text="item.templateName"></td>
                            <td class="center col-sm-2" v-text="item.summary"></td>
                            <td class="center col-sm-2" v-text="item.createTime"></td>
                            <td class="center col-sm-1" v-text="item.showChannelType"></td>
                            <td class="center col-sm-2" v-text="item.endTime"></td>
                            <td class="center col-sm-1" v-text="item.creator"></td>
                            <td class="center col-sm-1">
                                <a href="javascript:;" @click="showDelete(index)">删除</a>
                                /
                                <a href="javascript:;" @click="showUpdate(index)">修改</a>
                            </td>
                        </tr>
                    </template>
                    <template v-else>
                        <tr>
                            <td class="center" v-text="'没有数据'" colspan="8"></td>
                        </tr>
                    </template>
                    </tbody>
                </table>
                <!--底部页码-->
                <div v-if="viewData.length>0" class="row"
