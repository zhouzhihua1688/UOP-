                    <!--主表格新增/修改 弹窗-->
                    <div class="modal" id="add" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                        <div class="modal-dialog"  >
                            <div class="modal-content">
                                <div class="modal-header" style="padding: 10px;">
                                    <button type="button" class="close" data-dismiss="modal"
                                        aria-hidden="true">×</button>
                                    <h4 class="modal-title">创建运营计划</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="form " style="padding: 0 50px 0 24px;">
                                        <div class="form-group">
                                            <!-- <label class="control-label" style="width: 150px;text-align: right">
                                                触达方式名称:</label> -->
                                            <input type="text" style="width: 60%;" v-model.trim="diamodelName"
                                                placeholder="请输入计划名称" />
                                        </div>
                                        <div class="form-group">
                                            <!-- <label class="control-label" style="width: 150px;text-align: right">
                                                关键字:</label> -->
                                            <input type="text" style="width: 60%;" v-model.trim="diamodelDesc"
                                                placeholder="请输入计划描述" />
                                        </div>
                                        <div class="form-group">
                                            <!-- <label class="control-label" style="width: 150px;text-align: right">
                                                关键字:</label> -->
                                            <input type="text" style="width: 60%;" v-model.trim="diamodelKey"
                                                placeholder="请输入modelKey" />
                                        </div>
                                        <div class="form-group" style="position: relative;">
                                            <label class="control-label"> 预计起止时间:</label>
                                                <input type="text" id="startTime" class="date-timepicker"
                                                style="width: 145px; margin-left: 2px;"> <span>至</span>
                                                 <input type="text" id="endTime" class="date-timepicker"
                                                    style="width: 145px; margin-left: 2px;"> 
                                        </div>
                                    </div>
                                </div>
                                <p class="text-center">
                                    <button class="btn btn-sm btn-primary" @click="add">保存</button>
                                    <button class="btn btn-default btn-sm" data-dismiss="modal">取消</button>
                                </p>
                            </div>
                        </div>
                    </div>
                    <!--提醒表格新增/修改 弹窗-->
                    <div class="modal" id="warn" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                        <div class="modal-dialog"  >
                            <div class="modal-content">
                                <div class="modal-header" style="padding: 10px;">
                                    <button type="button" class="close" data-dismiss="modal"
                                        aria-hidden="true">×</button>
                                    <h4 class="modal-title">添加提醒</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="form " style="padding: 0 50px 0 24px;">
                                        <div class="form-group">
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                提醒人手机号:</label>
                                            <input type="text" style="width: 60%;" v-model.trim="warn.warnPhone"
                                                placeholder="多个手机号逗号分开" />
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label" style="width: 150px;text-align: right">
                                                短信:</label>
                                            <textarea name="" id="" cols="30" rows="10" style="width: 60%;height: 100px;  " placeholder="请输入短信内容" v-model.trim="warn.warnContent"></textarea>
                                        </div>
                                        <div class="form-group">
                                            <label class="control-label" style="width: 150px;text-align: right"> 提醒时间:</label>
                                            <div v-show="diatakeEffectType=='02'" style="display: inline-block;">
                                                <select name="" id="warnCycle" v-model="warnCycle">
                                                    <option value="day">每天</option>
                                                    <option value="week">每周</option>
                                                    <option value="month">每月</option>
                                                </select>
                                                <span v-show="warnCycle=='week'" style="margin-right: 15px;">
                                                    <select multiple="" name="warnWeekTime" id="warnWeekTime" class="select2"  >
                                                        <option value="2">周一</option>
                                                        <option value="3">周二</option>
                                                        <option value="4">周三</option>
                                                        <option value="5">周四</option>
                                                        <option value="6">周五</option>
                                                        <option value="7">周六</option>
                                                        <option value="1">周日</option>
                                                    </select>
                                                </span>
                                                <span  v-show="warnCycle=='month'" style="margin-right: 15px;">
                                                    <select multiple="" name="warnMonthday" id="warnMonthday" class="select2"  >
                                                        <option value="1">1</option>
                                                        <option value="2">2</option>
                                                        <option value="3">3</option>
                                                        <option value="4">4</option>
                                                        <option value="5">5</option>
                                                        <option value="6">6</option>
                                                        <option value="7">7</option>
                                                        <option value="8">8</option>
                                                        <option value="9">9</option>
                                                        <option value="10">10</option>
                                                        <option value="11">11</option>
                                                        <option value="12">12</option>
                                                        <option value="13">13</option>
                                                        <option value="15">15</option>
                                                        <option value="16">16</option>
                                                        <option value="17">17</option>
                                                        <option value="18">18</option>
                                                        <option value="19">19</option>
                                                        <option value="20">20</option>
                                                        <option value="21">21</option>
                                                        <option value="22">22</option>
                                                        <option value="23">23</option>
                                                        <option value="24">24</option>
                                                        <option value="25">25</option>
                                                        <option value="26">26</option>
                                                        <option value="27">27</option>
                                                        <option value="28">28</option>
                                                        <option value="29">29</option>
                                                        <option value="30">30</option>
                                                        <option value="31">31</option>
                                                    </select>
                                                </span>
                                                <input type="text" id="warnTime2" class="date-timepicker"
                                                  style="width: 145px; margin-left: 2px;">
                                            </div>
                                            <div v-show="diatakeEffectType=='01'" style="display: inline-block;">
                                                <input type="text" id="warnTime1" class="date-timepicker" style="width: 145px; margin-left: 2px;">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <p class="text-center">
                                    <button class="btn btn-sm btn-primary" data-dismiss="modal" @click="addWarnData">保存</button>
                                    <button class="btn btn-default btn-sm" data-dismiss="modal">取消</button>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="modal" id="createBlockView" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static" >
                        <div class="modal-dialog"  style="width: 60%;">
                            <div class="modal-content">
                                <div class="modal-header" style="padding: 10px;">
                                    <button type="button" class="close" data-dismiss="modal"
                                        aria-hidden="true">×</button>
                                    <h4 class="modal-title">创建子计划</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="widget-box">
                                        
                                        
                                        <div class="widget-body">
                                            <div class="widget-main">
                                                <div id="fuelux-wizard-container" class="fuelux-wizard-container">
                                                    <div class="form-group">
                                                        <label class="col-xs-12 col-sm-3 control-label no-padding-right" style="text-align: right;line-height: 32px;">
                                                            任务名称</label>
                                                        <input type="text" style="width: 60%;margin-left: 12px;" v-model.trim="diablockName"
                                                            placeholder="请输入任务名称" />
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-xs-12 col-sm-3 control-label no-padding-right" style="text-align: right;line-height: 32px;">
                                                            展示位置</label>
                                                        <input type="number" style="width: 60%;margin-left: 12px;" v-model.trim="diastepNumber"
                                                            placeholder="请输入位置（整数）" />
                                                    </div>
                                                    <div class="form-group">
                                                        <label class="col-xs-12 col-sm-3 control-label no-padding-right">
                                                            </label>
                                                        <textarea name="" id="" cols="30" rows="10" style="width: 60%;height: 100px;margin-left: 12px;" placeholder="请输入任务简述" v-model.trim="diablockDesc"></textarea>
                                                    </div>
                                                    <hr />
                                                    <div>
                                                        <ul class="steps">
                                                            <li data-step="1" class="active">
                                                                <span class="step">1</span>
                                                                <span class="title">触发配置</span>
                                                            </li>
    
                                                            <li data-step="2">
                                                                <span class="step">2</span>
                                                                <span class="title">受众用户</span>
                                                            </li>
    
                                                            <li data-step="3">
                                                                <span class="step">3</span>
                                                                <span class="title">触达配置</span>
                                                            </li>
<!--     
                                                            <li data-step="4">
                                                                <span class="step">4</span>
                                                                <span class="title">任务提醒</span>
                                                            </li> -->
                                                        </ul>
                                                    </div>
    
                                                    <hr />
    
                                                    <div class="step-content pos-rel">
                                                        <div class="step-pane active" data-step="1">
    
                                                            <form class="form-horizontal" id="">
                                                                <div class="form-group">
                                                                    <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right">触发类型</label>
                                                                    <div class="col-xs-12 col-sm-5 triggerType" >
                                                                        <p class="btn btn-info">触发型</p>
                                                                        <p class="btn ">定时型</p>
                                                                    </div>
                                                                </div>
                                                                <div class="type1" >
                                                                    <div class="form-group">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right" style="margin-left: 12px;"></label>
                                                                        <div  style="display: inline-block;">
                                                                            <span>完成事件</span>
                                                                            <select name="" id="" class="paddingL12" v-model="diaeventId">
                                                                                <option v-for="(eventItem,eventIndex) in eventData" :value="eventItem.eventId" :key="eventIndex" v-text="eventItem.eventName"></option>
                                                                            </select>
                                                                        </div>
                                                                        <span class="btn btn-minier btn-primary" v-show="diaeventId&&selectForNodeIds.length>0" style="vertical-align: middle;" @click="eventShow">
                                                                            <i class="ace-icon glyphicon glyphicon-plus"></i>
                                                                            筛选条件
                                                                        </span>
                                                                    </div> 
                                                                    <template v-if="!isUpdate1">
                                                                        <div class="form-group "  v-show="diaeventId&&eventForNodeIds.length>0" style="display: flex;align-items: center;">
                                                                            <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right"></label>
                                                                            <span class="" style="margin:0 0 0 12px;padding:0 3px;background-color: rgb(217, 237, 247);border: 1px #6FB3E0 solid;cursor: pointer;" v-text="logicFlag?'且':'或'" @click="andOrSwitch"></span>
                                                                            <span style="color:#6FB3E0">----</span>
                                                                            <div class=" triggerConditionExpression" style="padding: 10px;background-color: #d9edf7;">
                                                                                <div v-for="(nodeItem,nodeindex) in eventForNodeIds" :key="nodeindex" class="forTriggerCondition">
                                                                                    <select :name="nodeindex" id="" style="padding-right: 12px;"  v-model="nodeItem.key1=nodeItem.pkey">
                                                                                        <option v-for="(nodeSelectItem,nodeSelectIndex) in selectForNodeIds" :value="nodeSelectItem.pkey" v-text="nodeSelectItem.pname"></option>
                                                                                    </select>
                                                                                    <select :name="nodeindex" id="" style="padding-right: 12px;" v-model="nodeItem.key2">
                                                                                        <option value="==">等于</option>
                                                                                        <option value=">">大于</option>
                                                                                        <option value="<">小于</option>
                                                                                        <option value=">=">大于等于</option>
                                                                                        <option value="<=">小于等于</option>
                                                                                        <option value="!=">不等于</option>
                                                                                    </select>
                                                                                    <input type="text" :name="nodeindex" v-model="nodeItem.key3" />       
                                                                                    <a class="btn btn-grey btn-minier" @click="removeChild(nodeindex)">
                                                                                        <i class="ace-icon fa fa-trash-o fa-2x icon-only"></i>
                                                                                    </a>                                                            
                                                                                </div>
                                                                                    
                                                                            </div>
                                                                        </div>
                                                                    </template>
                                                                    <template v-else>
                                                                        <div class="form-group "  v-show="diaeventId&&eventForNodeIds.length>0" style="display: flex;align-items: center;">
                                                                            <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right"></label>
                                                                            <span class="" style="margin:0 0 0 12px;padding:0 3px;background-color: rgb(217, 237, 247);border: 1px #6FB3E0 solid;cursor: pointer;" v-text="logicFlag?'且':'或'" @click="andOrSwitch"></span>
                                                                            <span style="color:#6FB3E0">----</span>
                                                                            <div class=" triggerConditionExpression" style="padding: 10px;background-color: #d9edf7;">
                                                                                <div v-for="(nodeItem,nodeindex) in eventForNodeIds" :key="nodeindex" class="forTriggerCondition" >
                                                                                    <select :name="nodeindex" id="" style="padding-right: 12px;"  v-model="nodeItem.key1">
                                                                                        <option v-for="(nodeSelectItem,nodeSelectIndex) in selectForNodeIds" :value="nodeSelectItem.pkey" v-text="nodeSelectItem.pname"></option>
                                                                                    </select>
                                                                                    <select :name="nodeindex" id="" style="padding-right: 12px;" v-model="nodeItem.key2">
                                                                                        <option value="==">等于</option>
                                                                                        <option value=">">大于</option>
                                                                                        <option value="<">小于</option>
                                                                                        <option value=">=">大于等于</option>
                                                                                        <option value="<=">小于等于</option>
                                                                                        <option value="!=">不等于</option>
                                                                                    </select>
                                                                                    <input type="text" :name="nodeindex" v-model="nodeItem.key3" />      
                                                                                    <a class="btn  btn-minier" @click="removeChild(nodeindex)">
                                                                                        <i class="ace-icon fa fa-trash-o fa-2x icon-only"></i>
                                                                                    </a>                                                                           
                                                                                </div>
                                                                                    
                                                                            </div>
                                                                        </div>
                                                                    </template>
                                                                    <div class="form-group clearfix">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label " ></label>
                                                                        <div class="fl paddingL12">
                                                                            <span>则</span>
                                                                            <select name="" id="">
                                                                                <option value="">立即</option>
                                                                            </select>
                                                                            <span>受众用户进行触达</span>
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group clearfix">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label " ></label>
                                                                        <div class="fl paddingL12">
                                                                            <span>使用</span>
                                                                            <select name="" id="" v-model="diablockRuleDefindedBy">
                                                                                <option value="1">任务规则</option>
                                                                                <option value="2">用户自定义规则</option>
                                                                            </select>
                                                                            <span>进行条件判断</span>
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right">起止时间</label>
                                                                        <div class="col-xs-12 col-sm-8" >
                                                                            <input type="text" id="triggerTime1" class="date-timepicker"
                                                                                style="width: 145px; margin-left: 2px;">
                                                                            <span>至</span>
                                                                            <input type="text" id="triggerTime2" class="date-timepicker"
                                                                                style="width: 145px; margin-left: 2px;">
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right">参与限制</label>
                                                                        <div class="col-xs-12 col-sm-5" style="line-height: 36px;">
                                                                            <input class="ace " type="checkbox" checked>
                                                                            <span class="lbl"> 
                                                                                允许同一个用户多次参与该计划
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right"></label>
                                                                        <div class="col-xs-12 col-sm-9">
                                                                            <span>同一用户</span>
                                                                            <input type="number" style="width: 60px;" v-model="diauserParticipationCycle"/>
                                                                            <span>天内最多参与</span>
                                                                            <input type="number" style="width: 60px;" v-model="diaeachCycleParticipationTimes"/>
                                                                            <span>次</span>
                                                                        </div>
                                                                    </div>
                                                                   
                                                                </div>
                                                                <div class=" type2" style="display: none;">
                                                                    <div class="form-group">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right"></label>
                                                                        <div  class="col-xs-12 col-sm-5 ">
                                                                            <input type="radio" value="01" name="triggerType"  v-model="diatakeEffectType">单次
                                                                            <input type="radio" value="02" name="triggerType" v-model="diatakeEffectType">重复
                                                                        </div>
                                                                    </div>
                                                                    <div class="form-group">
                                                                        <label for="" class="col-xs-12 col-sm-3 control-label no-padding-right" style="margin-left: 12px;"></label>
                                                                        <div  style="display: inline-block;">
                                                                            <span>完成事件</span>
                                                                            <select name="" id="" class="paddingL12" v-model="diaeventId">
                                                                                <option v-for="(eventItem,eventIndex) in eventData" :value="eventItem.eventId" :key="eventIndex" v-text="eventItem.eventName"></option>
                                                                            </select>
                                                                        </div>
                                                                        <span class="btn btn-minier btn-primary" v-show="diaeventId&&selectForNodeIds.length>0" style="vertical-align: middle;" @click="eventShow">
                                                                            <i class="ace-icon glyphicon glyphicon-plus"></i>
                                                                            筛选条件
                                                                        </span>
                                                                    </div> 
                                                                    <template v-if="!isUpdate1">
                                                                        <div class="form-group "  v-show="diaeventId&&eventForNodeIds.length>0" style="display: flex;align-items: center;">
