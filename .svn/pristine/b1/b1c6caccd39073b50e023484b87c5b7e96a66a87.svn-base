                                    <button type="button" class="btn btn-primary btn-sm"
                                        data-dismiss="modal" @click="deleteSubConfirm">确定</button>
                                </p>
                            </div>
                        </div>
                    </div>
                    <!--父任务删除提示窗-->
                    <div class="modal" id="deleteModel" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
