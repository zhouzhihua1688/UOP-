                                                           <!-- <label class="btn" v-if='modifyStatus==1&&shareCoverImage' @click='delImg(shareCoverImage,"3")'>删除图片</label> -->
                                                           <label class="btn" v-if='modifyStatus==1&&shareCoverImage' @click='fakeDel("img")'>删除图片</label>

                                                    <!--<label v-if="shareCoverImage">图片路径:{{shareCoverImage}}</label>-->
