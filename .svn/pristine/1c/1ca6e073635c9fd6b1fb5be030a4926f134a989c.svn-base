                                                                         style="overflow: hidden;  position: relative;width:150px;height: 150px;">
                                                                        <img class="img" :src="v"
                                                                             style="margin:1rem;;width:130px;height: 130px;object-fit: contain"
                                                                             @click="fakeDel('3',i)">
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
