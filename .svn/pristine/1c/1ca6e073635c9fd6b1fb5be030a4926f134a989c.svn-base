                                                                     :output-type="option.outputType" :info="true"
                                                                     :full="option.full"
                                                                     :can-move="option.canMove"
                                                                     :can-move-box="option.canMoveBox"
                                                                     :fixed-box="option.fixedBox"
                                                                     :original="option.original"
                                                                     :auto-crop="option.autoCrop"
                                                                     :auto-crop-width="option.autoCropWidth"
                                                                     :auto-crop-height="option.autoCropHeight"
                                                                     :center-box="option.centerBox"
                                                                     @real-time="realTime" :high="option.high"
                                                                     mode="cover"
                                                                     :max-img-size="option.max"></vue-cropper>
                                                    </div>
                                                    <div class="preview">
                                                        <p>剪切实时预览：</p>
                                                        <div class="show-preview"
                                                             :style="{'width': previews.w + 'px', 'height': previews.h + 'px',  'overflow': 'hidden', 'margin': '5px','border':'1px #999 dashed'}">
                                                            <div :style="previews.div">
                                                                <img :src="previews.url" :style="previews.img">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </template>

                                    <!--<template v-if="addCategoryType=='DX'||addCategoryType=='MTBD'">-->
                                    <template v-if="addCategoryType=='MTBD'">
                                        <template v-if="contentType!='WORD'&&contentType!='LINK'">
                                            <div class="form-group">
                                                <label v-if='contentType=="FILE"||!contentType'
                                                       class="col-sm-2 control-label">
                                                    *内容文件:</label>
                                               

                                                <label v-if='contentType=="PIC"' class="col-sm-2 control-label">
                                                    *内容图片:</label>
                                                <div class="col-sm-8">
                                                    <div v-if='contentType=="FILE"||!contentType'>
                                                        <label class="btn" for="uploadsFile">上传文件</label>
                                                        <input v-model="fileUrl" style="width:80%" disabled><!-- 只是显示-->
                                                        <input type="file"
                                                               style="position:absolute; clip:rect(0 0 0 0);"
                                                               id="uploadsFile" accept=""
                                                               @change="uploadsFile($event)">
                                                        <!-- <i v-if='fileUrl' @click='delImg(fileUrl,"2")'>x</i> -->
                                                        <i v-if='fileUrl' @click='fakeDel("file")'>x</i>

                                                        <!-- 进度条 -->
                                                        <div style="display: flex; align-items:end;margin-top: 20px;"
                                                             v-show='uploadingFile.name'>
                                                            <div>
                                                                <div style="display: flex;justify-content: space-between;">
                                                                    <span>{{uploadingFile.name}}</span>
                                                                    <span >
                                                                        <i v-if='uploadingFile.progressRate=="100%"' style="padding:0 10px;cursor:pointer;">✓</i>
                                                                        <!-- <i v-if='fileUrl' @click='delImg(fileUrl,"2")' style="padding:0 10px;cursor:pointer;">x</i> -->
                                                                        <i v-if='fileUrl' @click='fakeDel("file")' style="padding:0 10px;cursor:pointer;">x</i>
