<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../../common/head.html %>
    <!-- inline styles related to this page -->
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-datetimepicker.min.css"/>
    <link rel="stylesheet" href="/css/chosen.min.css">
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-multiselect.min.css"/>
    <style>
        .table > tbody > tr > td {
            vertical-align: middle;
        }

        .form-inline > div {
            padding: 10px 0;
            text-align: right;

        }

        .form-inline span {
            text-align: left;
        }

        @media (min-width: 768px) {
            .modal-dialog {
                width: 1000px;
                margin: 30px auto;
            }
        }

        #wangEditor .w-e-text p {
            white-space: break-spaces;
        }

        .btn-group-xs > .btn, .btn-xs {
            padding: 0 !important;
            border-radius: 3px;
        }

        input[type=checkbox].ace.ace-switch.ace-switch-4, input[type=checkbox].ace.ace-switch.ace-switch-5 {
            width: 80px;
        }

        input[type=checkbox].ace.ace-switch.ace-switch-5 + .lbl::before {
            width: 80px;
            content: "开启\a0\a0\a0\a0\a0\a0\a0\a0\a0\a0\a0关闭";
        }

        input[type=checkbox].ace.ace-switch.ace-switch-4:checked + .lbl::after, input[type=checkbox].ace.ace-switch.ace-switch-5:checked + .lbl::after {
            left: 59px;
        }

        .no-select {
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }

        .cut {
            width: 400px;
            height: 400px;
            border: 1px #999 dashed;
            margin-right: 40px;
        }

        .flex-cropper {
            margin-top: 14px;
            display: flex;
        }

        [v-cloak] {
            display: none;
        }

        .dropdown-menu {
            z-index: 99;
        }

        .messageType {
            display: inline-block;
        }

        .messageType:nth-of-type(2) {
            margin-left: 20px
        }

        .messageType:nth-of-type(3) {
            margin-left: 20px
        }

        .messageType:nth-of-type(4) {
            margin-left: 20px
        }

        /* 进度条样式 */
        .g-container {
            width: 600px;
            height: 10px;
            border-radius: 5px;
            background: #f2f2f2;
        }

        .g-progress {
            width: 0;
            height: inherit;
            border-radius: 5px;
            background: #999;
        }

        .cover {
            position: fixed;
            left: 0;
            top: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(9, 10, 10, 0.527);
            z-index: 999;
        }
    </style>
</head>

<body class="no-skin">
<% include ../../common/header.html %>
<div class="main-container ace-save-state" id="main-container">
    <% include ../../common/sidebar.html %>
    <% include ../../common/left-menu.html %>
    <div class="main-content">
        <div class="main-content-inner">
            <div class="page-content">
                <% include ../../common/breadcrumb.html %>
                <% include ../../common/ace-settings-container.html %>
                <!-- loading动画 -->
                <div class="cover" v-show="loadingShow" style="z-index: 1999;display: none;">
                    <img src="/img/loading.gif" alt="加载数据中" style="position: absolute;top: 50%;left: 50%;">
                    <div>数据加载中...</div>
                </div>
                <!-- loading动画end -->
                <div id="content">
                    <!--提示信息弹窗-->
                    <div class="modal" id="info" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                        <div class="modal-dialog" style="width:600px;">
                            <div class="modal-content">
                                <div class="modal-header" style="padding: 10px;">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×
                                    </button>
                                    <h4 class="modal-title">提示信息</h4>
                                </div>
                                <div class="modal-body text-center">
                                    <h4 v-text="diaMsg"></h4>
                                </div>
                                <p class="text-center">
                                    <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">确定
                                    </button>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="modal" id="delImg" tabindex="-1" role="dialog" aria-hidden="true"
                         data-backdrop="static">
                        <div class="modal-dialog" style="width:600px;">
                            <div class="modal-content">
                                <div class="modal-header" style="padding: 10px;">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×
                                    </button>
