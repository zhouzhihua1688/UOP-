                                                <input type="file" id="uploadsFile"
                                                       style="position:absolute; clip:rect(0 0 0 0);" accept=""
                                                       @change="uploadsFile($event)">
                                                <!-- 进度条 -->
                                                <div style="display: flex; align-items:end;margin-top :20px;"
                                                     v-show='uploadingFile.name'>
                                                    <div>
                                                        <div style="display: flex;justify-content: space-between;">
                                                            <span>{{uploadingFile.name}}</span>
                                                            <span >
                                                                <i v-if='uploadingFile.progressRate=="100%"' style="padding:0 10px;cursor:pointer;">✓</i>
                                                                <!-- <i v-if='fileUrl' @click='delImg(fileUrl,"2")' style="padding:0 10px;cursor:pointer;">x</i> -->
                                                                <i v-if='fileUrl' @click='fakeDel("file")' style="padding:0 10px;cursor:pointer;">x</i>
                                                            </span>
                                                        </div>

                                                        <div class="g-container">
                                                            <div class="g-progress"></div>
                                                        </div>
                                                    </div>

                                                    <div style='color:red;'>
                                                        <i v-if='uploadingFile.progressRate=="100%"&&fileUrl'> 上传完成</i>
                                                        <i v-else>上传中</i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </template>

                                    <!--当选择单选的类型为文字-->
                                    <div class="form-group" v-if="contentType=='WORD'">
                                        <label class="col-sm-2 control-label">
                                            *内容:</label>
                                        <div class="col-sm-7">
                                            <textarea name="" cols="15" rows="5"
                                                      v-model="word"
                                                      style="height:100px; width: 300px;"></textarea>
                                        </div>
                                    </div>
                                    <!--当选择单选的类型为链接-->
                                    <!--v-if="(addCategoryType=='DX'||addCategoryType=='MTBD')&&contentType=='LINK'"-->
                                    <div class="form-group"
                                         v-if="(addCategoryType=='MTBD')&&contentType=='LINK'">
                                        <label class="col-sm-2 control-label">
                                            *跳转URL:</label>
                                        <div class="col-sm-7">
                                            <input type="text" v-model="linkUrl" style="width:100%;">
                                        </div>
                                    </div>

                                    <!--<div class="form-group">-->
                                    <!--<label class="col-sm-2 control-label">-->
                                    <!--*专题底部文字:</label>-->
                                    <!--<div class="col-sm-9" id="wangEditor" style="z-index: 9;">-->
                                    <!--</div>-->
                                    <!--</div>-->

                                    <p class="text-center">
                                        <button class="btn btn-info btn-sm" type="button" @click="add()">
                                            确定
                                        </button>
                                        <button type="button" class="btn btn-danger btn-sm" data-dismiss="modal">
                                            取消
                                        </button>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!--PV弹窗-->
                    <div class="modal" id="pv" tabindex="0" role="dialog" aria-hidden="true" data-backdrop="static">
                        <div class="modal-dialog" style="width:1100px;">
                            <div class="modal-content">
                                <div class="modal-header" style="padding: 10px;">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;
                                    </button>
                                    <h4 class="modal-title">用户排名
                                        <button class="btn btn-default btn-xs" style="margin-left: 30px"
                                                @click="exportPvData"><i
                                                class="ace-icon fa-2x glyphicon glyphicon-share"></i></button>
                                    </h4>
                                    <!--<div class="form-group pull-left">-->
                                    <!--<button class="btn btn-default btn-xs "><i class="ace-icon fa-2x glyphicon glyphicon-share"></i></button>-->
                                    <!--</div>-->
                                </div>
                                <div class="modal-body">
                                    <div class="form-group  pull-left">
                                        <label class="control-label">选择用户类型:</label>
                                        <select style="display:inline-block;width:150px" v-model="roleId">
                                            <option value="">请选择</option>
                                            <option :value="item.roleId" v-for="(item,index) in userTypeList">
                                                {{item.roleId}}-{{item.roleName}}
                                            </option>
