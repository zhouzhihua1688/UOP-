                                    </div>
                                    <!--当选择内容类型为营销海报-->
                                    <div class="form-group" v-if="addCategoryType=='YXHB'">
                                        <label class="col-sm-2 control-label">
                                            *海报日期:</label>
                                        <div class="col-sm-7">
                                            <date-picker id="posterDate" v-model='posterDate'
                                                         :config="{placeholder:'',format: 'YYYY-MM-DD'}">
