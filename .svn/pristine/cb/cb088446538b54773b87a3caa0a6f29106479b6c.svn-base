        #base label {
            width: 144px;
            text-align: right;
        }

        #restrict label {
            width: 144px;
            text-align: right;
        }

        #operation table tr td:nth-child(1) {
            background-color: #eee;
        }

        #detail table tr td:nth-child(odd) {
            background-color: #eee;
        }

        #operation table tr td:nth-child(2) input {
            width: 100%;
        }

        #operation table tr td:nth-child(5) input {
            width: 100%;
        }

        #operation table tr td:nth-child(4) {
            background-color: #eee;
        }
    </style>
