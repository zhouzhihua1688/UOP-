                            <!-- DIALOG ENDS -->
                            <div class="form-inline" style="padding: 10px 0;">
                                <div class="form-group">
                                    <label style="width: 60px;">基金名称:</label>
                                    <select style="width: 100px;" v-model="classify">
                                        <option value="ALL">全部</option>
                                        <option value="6">专户</option>
                                        <option value="9">子公司</option>
                                        <option value="OTHER">其他</option>
                                    </select>
                                    <!-- <select style="width: 169px;" v-model="fundid"></select> -->

                                    <vue-select :options='[{fundName:"请选择",fundId:""},...lineFundIdList]' 
                                        v-model='qFundid' label='fundName' model='fundId'>
                                    </vue-select>
                                </div>

                                <div class="form-group pull-right">
                                    <input type="button" class="btn btn-sm btn-primary " value="查询"
                                        @click="getTableData(0)">
                                </div>
                            </div>


                            <div class="table-header">
                                <%= cpage %>--表格数据
                            </div>
                            <!--分页器和快速查询-->
                            <div class="row" style="padding:8px 0;background-color: #EFF3F8; margin: 0;">
                                <div class="col-xs-6">
                                    <div class="dataTables_length" id="dynamic-table_length">
                                        <label>每页显示</label>
                                        <select class="input-sm" name="pageMaxNum" v-model.trim="pageMaxNum">
                                            <option value="10">10</option>
                                            <option value="20">20</option>
                                            <option value="50">50</option>
                                        </select>
                                        <label>条</label>
                                    </div>
                                </div>
                            </div>
                            <!--数据表-->
                            <table id="simple-table" class="table table-bordered table-hover dataTable">
                                <thead>
                                    <tr>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">产品代码</th>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">基金名称</th>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">TA代码</th>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">基金类别</th>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">投资方向</th>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">是否上架</th>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">显示顺序</th>
                                        <th class="center col-sm-1 col-md-1 col-lg-1">操作</th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <template v-if="viewData.length>0">
                                        <tr v-for="(item,index) in viewData" :key="index">
                                            <td class="center" v-text='item.fundId'></td>
                                            <td class="center" v-text='item.fundInfo? item.fundInfo.fundnm:""'></td>
                                            <td class="center" v-text='item.fundInfo? item.fundInfo.tano:""'></td>
                                            <td class="center"
                                                v-text='item.fundExtension? item.fundExtension.fundtp:""'>
                                            </td>
                                            <td class="center"
                                                v-text='item.fundExtension? item.fundExtension.investtype:""'>
                                            </td>
                                            <td class="center" v-text='item.fundDetail? item.fundDetail.onsaleflag: ""'>
                                            </td>
                                            <td class="center" v-text='item.fundInfo? item.fundInfo.displayorder:""'>
                                            </td>
                                            <td class="center">
                                                <a href="javascript:;"
                                                    @click='showFileData(item.fundId,"operation")'>修改</a>
                                                <span style="margin:0 5px;">/</span>
                                                <a href="javascript:;"
                                                    @click='showFileData(item.fundId,"detail")'>详情</a>
                                            </td>

                                        </tr>
                                    </template>
                                    <template v-else>
                                        <tr>
                                            <td class="center" v-text="'没有数据'" colspan="11"></td>
                                        </tr>
                                    </template>
                                </tbody>
                            </table>

                            <!--底部页码-->
                            <div v-if="viewData.length>0" class="row"
                                style="border-bottom: 1px solid #e0e0e0;padding-top: 12px;padding-bottom: 12px;background-color: #EFF3F8;margin: 0;">
                                <div class="col-xs-2">
                                    <div class="dataTables_info" v-text="'共 '+middleData.length+' 页'"></div>
                                </div>
                                <div class="col-xs-10">
                                    <div class="dataTables_paginate paging_simple_numbers" id="dynamic-table_paginate">
                                        <ul class="pagination">
                                            <li class="paginate_button"
                                                v-show="pageList.length<middleData.length&&currentIndex>maxSpace">
                                                <a href="javascript:;" v-text="'1...'" @click="toFirst"></a>
                                            </li>
                                            <li :class="currentIndex==0?'paginate_button previous disabled':'paginate_button previous'"
                                                @click="prev">
                                                <a href="javascript:;" v-text="'上一页'"></a>
                                            </li>
                                            <li :class="value==currentIndex+1?'paginate_button active':'paginate_button'"
                                                v-for="value in pageList" @click="changeIndex(value)">
                                                <a href="javascript:;" v-text="value"></a>
                                            </li>
                                            <li class="paginate_button"
                                                v-show="pageList.length<middleData.length&&middleData.length-currentIndex>maxSpace">
                                                <a href="javascript:;" v-text="'...'+middleData.length"
                                                    @click="toLast"></a>
                                            </li>
                                            <li :class="currentIndex==middleData.length-1?'paginate_button next disabled':'paginate_button next'"
                                                @click="next">
                                                <a href="javascript:;" v-text="'下一页'"></a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>


                        </div>

                    </div><!-- /.row -->
                </div><!-- /.page-content -->
            </div>
        </div><!-- /.main-content -->
