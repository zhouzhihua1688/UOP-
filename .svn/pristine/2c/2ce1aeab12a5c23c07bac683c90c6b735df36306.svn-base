                                                <a href="javascript:;" @click="localUpdate(item)">新增修改</a>
                                                <a href="javascript:;" @click="showRevoke(item)" style="color:red">/ 撤销</a>
                                            </template>
                                            <template v-if="item.calc_status=='false'">
                                                <span>-</span>
                                            </template>
                                        </template>
                                        <template v-else-if="item.operate==2">
                                            <template v-if="item.fundgroupNewFundgroupDO">
                                                <a href="javascript:;" @click="localUpdate(item)">修改信息</a>
                                            </template>
                                            <template v-else-if="item.dialogData&&(item.mysqlStatus!='2'&&item.mysqlStatus!='1')">
                                                <a href="javascript:;" @click="showLocalAllUpdate(item)">全量修改</a>/
                                            </template>
                                            <template v-else>-</template>
                                            <!-- <template v-else v-show="item.mysqlStatus!='2'">
                                                <a href="javascript:;" @click="houseUpdate(item)">修改调仓</a>
                                            </template>
                                            / -->
                                            <a href="javascript:;" v-if="item.mysqlStatus!='2'&&item.mysqlStatus!='1'" @click="showRevoke(item)" style="color:red">撤销</a>
                                        </template>
                                        <!-- /<a href="javascript:;" @click="showRisk(item)">风控配置</a> -->
                                        <!--<span>/</span>-->
                                    </template>

                                </template>
                                <template v-else-if="item.mysqlStatus==9">    
                                    <!--<a href="javascript:;" @click="showSubmitAgain(item)">重新提交</a>-->

                                    <template v-if="item.operate==1&&!item.dialogData">
                                        <a href="javascript:;" @click="showSubmitAgain(item)">重提新增</a>
                                    </template>
                                    <template v-else="item.operate==2">
                                        <template v-if="item.dialogData">
                                            <a href="javascript:;" @click="showLocalAllUpdate(item)">重提修改</a>/
                                            <a href="javascript:;" @click="showRevoke(item)" style="color:red">撤销</a>
                                        </template>
                                        <!-- <template v-if="item.fundgroupNewFundgroupDO">
                                            <a href="javascript:;" @click="showSubmitAgain(item)">重提修改</a>
                                        </template>
                                        <template v-else>
                                            <a href="javascript:;" @click="showSubmitAgain(item)">重提调仓</a>
                                        </template> -->
                                    </template>
                                </template>
                                <template v-else>
                                    <span>-</span>
                                </template>
                                <% } %>
                            </td>
                        </tr>
                    </template>
                    <template v-else>
                        <tr>
                            <td class="center" v-text="loadingStatus" colspan="26"></td>
                        </tr>
                    </template>
                    </tbody>
                </table>
                <!--底部页码-->
                <!--底部假分页页码-->
                <div v-if="viewData.length>0" class="row" v-show="isUpdate"
                     style="border-bottom: 1px solid #e0e0e0;padding-top: 12px;padding-bottom: 12px;background-color: #EFF3F8;margin: 0;">
                    <div class="col-xs-2">
                        <div class="dataTables_info" v-text="'共 '+middleData.length+' 页'"></div>
                    </div>
                    <div class="col-xs-10">
                        <div class="dataTables_paginate paging_simple_numbers" id="dynamic-table_paginate">
                            <ul class="pagination">
                                <li class="paginate_button"
                                    v-show="pageList.length<middleData.length&&currentIndex>maxSpace">
                                    <a href="javascript:;" v-text="'1...'" @click="toFirst1"></a>
                                </li>
                                <li :class="currentIndex==0?'paginate_button previous disabled':'paginate_button previous'"
                                    @click="prev1">
                                    <a href="javascript:;" v-text="'上一页'"></a>
                                </li>
                                <li :class="value==currentIndex+1?'paginate_button active':'paginate_button'"
                                    v-for="value in pageList" @click="changeIndex1(value)">
                                    <a href="javascript:;" v-text="value"></a>
                                </li>
                                <li class="paginate_button"
                                    v-show="pageList.length<middleData.length&&middleData.length-currentIndex>maxSpace">
                                    <a href="javascript:;" v-text="'...'+middleData.length" @click="toLast1"></a>
                                </li>
                                <li :class="currentIndex==middleData.length-1?'paginate_button next disabled':'paginate_button next'"
                                    @click="next1">
                                    <a href="javascript:;" v-text="'下一页'"></a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!--底部假分页页码-->
                <div v-if="viewData.length>0" class="row" v-show="showMysql"
                     style="border-bottom: 1px solid #e0e0e0;padding-top: 12px;padding-bottom: 12px;background-color: #EFF3F8;margin: 0;">
                    <div class="col-xs-2">
                        <div class="dataTables_info" v-text="'共 '+middleData.length+' 页'"></div>
                    </div>
                    <div class="col-xs-10">
                        <div class="dataTables_paginate paging_simple_numbers" id="dynamic-table_paginate">
                            <ul class="pagination">
                                <li class="paginate_button"
                                    v-show="pageList.length<middleData.length&&currentIndex>maxSpace">
                                    <a href="javascript:;" v-text="'1...'" @click="toFirst"></a>
                                </li>
                                <li :class="currentIndex==0?'paginate_button previous disabled':'paginate_button previous'"
                                    @click="prev">
                                    <a href="javascript:;" v-text="'上一页'"></a>
                                </li>
                                <li :class="value==currentIndex+1?'paginate_button active':'paginate_button'"
                                    v-for="value in pageList" @click="changeIndex(value)">
                                    <a href="javascript:;" v-text="value"></a>
                                </li>
                                <li class="paginate_button"
                                    v-show="pageList.length<middleData.length&&middleData.length-currentIndex>maxSpace">
                                    <a href="javascript:;" v-text="'...'+middleData.length" @click="toLast"></a>
                                </li>
                                <li :class="currentIndex==middleData.length-1?'paginate_button next disabled':'paginate_button next'"
                                    @click="next">
                                    <a href="javascript:;" v-text="'下一页'"></a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div><!-- /.page-content -->
        </div>
