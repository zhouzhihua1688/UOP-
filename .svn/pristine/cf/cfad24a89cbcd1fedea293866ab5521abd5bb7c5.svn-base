                                                    </template>
                                                    <template v-else='checkParam.onsaleFlag==1'>
                                                        <input type="text" style="width:175px;" disabled value="上架"/>
                                                    </template>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">投资期限:</label>
                                                    <input type="text" style="width:175px;" disabled
                                                           v-model="checkParam.investPeriod"/>

                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">营销信息2(App):</label>
                                                    <input type="text" style="width:175px;" disabled
                                                           v-model="checkParam.remarkMb2"/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">Banner(手机):</label>
                                                    <input type="text" style="width:175px;margin-bottom:5px;" disabled
                                                           v-model="checkParam.attachBannerMb"/>
                                                </div>
                                            </div>
                                            <div style="display: inline-block;width: 33%;vertical-align:top;">
                                                <div class="form-group">
                                                    <label class="control-label">专区类型:</label>
                                                    <!-- <input type="text" style="width:175px;" disabled v-model="checkParam.zoneType"/> -->
                                                    <template v-if='checkParam.zoneType==0'>
                                                        <input type="text" style="width:175px;" disabled value="普通专区"/>
                                                    </template>
                                                    <template v-else='checkParam.zoneType==1'>
                                                        <input type="text" style="width:175px;" disabled value="品牌专区"/>
                                                    </template>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">上架时间:</label>
                                                    <!-- <input type="text" style="width:175px;" disabled v-model="checkParam.onsaleDate"/> -->
                                                    <input type="text" style="width:175px;" disabled
                                                           v-model="moment(checkParam.onsaleDate).format('YYYY-MM-DD')"/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">专区样式:</label>
                                                    <!-- <input type="text" style="width:175px;" disabled v-model="checkParam.displayStyle"/> -->
                                                    <template v-if='checkParam.displayStyle==0'>
                                                        <input type="text" style="width:175px;" disabled value="普通专区"/>
                                                    </template>
                                                    <template v-else='checkParam.displayStyle==1'>
                                                        <input type="text" style="width:175px;" disabled value="秒杀专区"/>
                                                    </template>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- 结束 -->

                                        <!-- 开始关联产品-->
                                        <div id="product2" class="tab-pane ">
                                            <!--<input type="button" class="btn btn-sm btn-primary pull-right" style="margin-bottom: 10px;margin-top: -10px;" value="新增">-->
                                            <table id="simple-table" class="table table-bordered table-hover dataTable">
                                                <thead>
                                                <tr>
                                                    <th class="center">基金代码
                                                    </th>
                                                    <th class="center">基金名称
                                                    </th>
                                                    <th class="center">显示顺序
                                                    </th>
                                                </tr>
                                                </thead>
                                                <tbody>
                                                <tr v-for="(item,index) in checkParam.hlvlFundZoneMappingList"
                                                    :key="index">
                                                    <td class="center" v-text="item.fundId">
                                                    </td>
                                                    <td class="center" v-text="item.fundNm">
                                                    </td>
                                                    <td class="center" v-text="item.displayOrder">
                                                    </td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- 结束 -->
                                    </div>
                                </div>
                            </div>
                            <p class="text-center" style="clear: both">
                                <!--<button class="btn btn-sm btn-primary" @click="saveParam()">保存</button>-->
                                <button type="button" class="btn btn-default btn-sm"
                                        data-dismiss="modal">取消
                                </button>
                            </p>
                        </div>
                    </div>
                </div>
                <!--删除弹窗-->
                <div class="modal" id="del" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                                <h4 class="modal-title">删除数据</h4>
                            </div>
                            <div class="modal-body text-center">
                                <h4>确认删除此条数据?</h4>
                            </div>
                            <p class="text-center">
                                <button type="button" class="btn btn-primary btn-sm" @click="deleteList()">确定</button>
                                <button type="button" class="btn btn-default btn-sm" data-dismiss="modal">取消</button>
                            </p>
                        </div>
                    </div>
                </div>
                <!--提示信息弹窗-->
                <div class="modal" id="info" tabindex="-1" role="dialog" aria-hidden="true"
                     data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal"
                                        aria-hidden="true">×
                                </button>
                                <h4 class="modal-title">提示信息</h4>
                            </div>
                            <div class="modal-body text-center">
                                <h4 v-text="diaMsg"></h4>
                            </div>
                            <p class="text-center">
                                <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">确定
                                </button>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="form-inline" style="padding:10px 0 40px 0;">
                    <!--<label class="control-label">客群名称:</label>-->
                    <!--<select style="width:175px" v-model="custNo">-->
                    <!--<option value="">全部</option>-->
                    <!--<option :value="item.custGroupNo" v-for="(item,index) in custGroup">-->
                    <!--{{item.custGroupNo}}-->
                    <!--</option>-->
                    <!--</select>-->
                    <div class="form-group pull-right">
                        <input type="button" class="btn btn-sm btn-primary" value="查询" @click="getTableData(0)">
                        <input type="button" class="btn btn-sm btn-primary" value="新增" @click="showAdd();">
                        <input type="button" class="btn btn-sm btn-primary" value="删除" @click="showDelete()">
                    </div>
                </div>
                <div class="table-header">
                    <%= cpage %>--表格数据
                </div>
                <div>
                    <!--分页器和快速查询-->
                    <div class="row" style="padding:8px 0;background-color: #EFF3F8; margin: 0;">
                        <div class="col-xs-6">
                            <div class="dataTables_length" id="dynamic-table_length">
                                <label>每页显示</label>
                                <select class="input-sm" name="pageMaxNum" v-model="pageMaxNum">
                                    <option value="10">10</option>
                                    <option value="20">20</option>
                                    <option value="50">50</option>
                                </select>
                                <label>条</label>
                            </div>
                        </div>
                    </div>
                    <!--数据表-->
                    <table class="table table-striped table-bordered table-hover dataTable" style="margin-bottom: 0;">
                        <thead>
                        <tr>
                            <th class="center">
                                <label class="pos-rel">
                                    <input type="checkbox" class="ace" v-model="allCheck" @click="selectAll(allCheck)"/>
                                    <span class="lbl"></span>
                                </label>
                            </th>
                            <th class="center">专区代码</th>
                            <th class="center">专区名称</th>
                            <th class="center">专区类型</th>
                            <th class="center">显示顺序</th>
                            <th class="center">是否上架</th>
                            <th class="center">操作</th>
                        </tr>
                        </thead>
                        <tbody>
                        <!--真分页业务数据-->
                        <template v-if="tableData.length>0">
                            <tr v-for="(item,index) in tableData" :key="index"
                                :style="index%2==0?'background-color: #f9f9f9;':''">
                                <td class="center">
                                    <label class="pos-rel">
                                        <input type="checkbox" class="ace" v-model="item.check" @click="check(index)"/>
                                        <span class="lbl"></span>
                                    </label>
                                </td>
                                <td class="center" v-text="item.zoneId"></td>
                                <td class="center" v-text="item.zoneName"></td>
                                <td class="center">{{item.zoneType | zoneType}}</td>
                                <td class="center" v-text="item.displayOrder"></td>
                                <td class="center">{{item.onsaleFlag | onsaleFlag}}</td>
                                <td class="center">
                                    <a href="javascript:;" @click="checklist(item)">详情</a>&nbsp;/&nbsp;
                                    <a href="javascript:;" @click="showUpdate(item)">修改</a>
                                </td>
                            </tr>
                        </template>
                        <template v-else>
                            <tr>
                                <td class="center" v-text="'没有数据'" colspan="22"></td>
                            </tr>
                        </template>
                        </tbody>
                    </table>

                    <!--底部页码-->
                    <!--后端真分页-->
                    <div v-if="tableData.length>0" class="row"
                         style="border-bottom: 1px solid #e0e0e0;padding-top: 12px;padding-bottom: 12px;background-color: #EFF3F8;margin: 0;">
                        <div class="col-xs-2">
                            <div class="dataTables_info" v-text="'共 '+totalPage+' 页'"></div>
                        </div>
                        <div class="col-xs-10">
                            <div class="dataTables_paginate paging_simple_numbers" id="dynamic-table_paginate">
                                <ul class="pagination">
                                    <li class="paginate_button"
                                        v-show="pageList.length<totalPage&&currentIndex>maxSpace">
                                        <a href="javascript:;" v-text="'1...'" @click="toFirst"></a>
                                    </li>
                                    <li :class="currentIndex==0?'paginate_button previous disabled':'paginate_button previous'"
                                        @click="prev">
                                        <a href="javascript:;" v-text="'上一页'"></a>
                                    </li>
                                    <li :class="value==currentIndex+1?'paginate_button active':'paginate_button'"
                                        v-for="value in pageList" @click="changeIndex(value)">
                                        <a href="javascript:;" v-text="value"></a>
                                    </li>
                                    <li class="paginate_button"
                                        v-show="pageList.length<totalPage&&totalPage-currentIndex>maxSpace">
                                        <a href="javascript:;" v-text="'...'+totalPage" @click="toLast"></a>
                                    </li>
                                    <li :class="currentIndex==totalPage-1?'paginate_button next disabled':'paginate_button next'"
                                        @click="next">
                                        <a href="javascript:;" v-text="'下一页'"></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- /.page-content -->
        </div>
    </div><!-- /.main-content -->
    <% include ../../common/footer.html %>
</div><!-- /.main-container -->
<% include ../../common/basic-scripts.html %>
<!-- 开发环境版本，包含了用帮助的命令行警告 -->
<script src="/js/lib/vue.js"></script>
<script src="/js/lib/ajaxfileupload.js"></script>
<script src="/ace/assets/js/bootstrap-timepicker.min.js"></script>
<script src="/ace/assets/js/moment.min.js"></script>
<script src="/js/lib/zh-cn.js"></script>
<script src="/ace/assets/js/bootstrap-datetimepicker.min.js"></script>
<script src="/js/businessMgmt/highFinancialMgmt/productAreaMgmt.js"></script>
</body>
</html>
