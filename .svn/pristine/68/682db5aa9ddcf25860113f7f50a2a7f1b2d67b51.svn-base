.control-label{
    position: relative
}
.filter{
    list-style: none;
    width: 168px;
    overflow-x: hidden;
    border: 1px solid #D5D5D5;
    position: absolute;
    top: 34px;
    left: 38px;
    background-color: #fff;
    z-index: 999;
    cursor: pointer
}
.filter li:hover{
    background-color: #307ecc;
    color: #fff
}
[v-cloak]{
    display: none;
}

.cover {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(9, 10, 10, 0.527);
    z-index: 999;
    }
    

input.typeaheada.scrollable~.tt-menu {
    max-height: 200px;
    overflow-y: auto;
}

input[class~="scrollable"]{
    width: 168px;
    min-width: 168px;
    height: 34px;
    font-size: 15px;
}

.table>tbody>tr>td{ vertical-align: middle;}