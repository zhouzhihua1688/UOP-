<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../../common/head.html %>
    <!-- inline styles related to this page -->
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-datetimepicker.min.css"/>
    <style>
        .table > tbody > tr > td {
            vertical-align: middle;
        }

        .upSlide {
            height: 300px;
            overflow-y: auto;
        }
    </style>
</head>
<body class="no-skin">
<% include ../../common/header.html %>
<div class="main-container ace-save-state" id="main-container">
    <% include ../../common/sidebar.html %>
    <% include ../../common/left-menu.html %>
    <div class="main-content">
        <div class="main-content-inner">
            <div class="page-content" id="content">
                <% include ../../common/breadcrumb.html %>
                <% include ../../common/ace-settings-container.html %>
                <!--提示信息弹窗-->
                <div class="modal" id="info" tabindex="-1" role="dialog" aria-hidden="true"
                     data-backdrop="static">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;">
                                <button type="button" class="close" data-dismiss="modal"
                                        aria-hidden="true">×
                                </button>
                                <h4 class="modal-title">提示信息</h4>
                            </div>
                            <div class="modal-body text-center">
                                <h4 v-text="diaMsg"></h4>
                            </div>
                            <p class="text-center">
                                <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">确定
                                </button>
                            </p>
                        </div>
                    </div>
                </div>

                <!--组合资产详情-->
                <div class="modal" id="details" tabindex="-1" role="dialog" aria-hidden="true"
                     data-backdrop="static">
                    <div class="modal-dialog" style="width:80%">
                        <div class="modal-content">
                            <div class="modal-header" style="padding: 10px;text-align:center">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                    &times;
                                </button>
                                <h4 class="modal-title" style="display: inline;"></h4>
                            </div>
                            <div class="modal-body">
                                <div class="form-group">
                                    <div :class="{'upSlide':combinationDirect.length>9}">
                                        <table id="index" class="table table-bordered table-hover dataTable ">
                                            <thead>
                                            <tr>
                                                <th class="center">{{productId}}</th>
                                                <th class="center">{{productName}}</th>
                                                <th class="center">资产</th>
                                                <th class="center">静态份额</th>
                                                <th class="center">净值(日期)</th>
                                                <th class="center">买入在途金额</th>
                                                <th class="center">卖出在途份额</th>
                                                <th class="center">赎回充值在途</th>
                                                <th class="center">分红在途金额</th>
                                                <th class="center">可用份额</th>
                                                <th class="center">持有成本</th>
                                                <th class="center">持有收益</th>
                                                <th class="center">持有收益率(%)</th>
                                                <th class="center">累计买入金额</th>
                                                <th class="center">累计收益</th>
                                                <th class="center">最新收益</th>
                                                <th class="center">当日计入手续费</th>
                                            </tr>
                                            </thead>
                                            <tbody>
                                            <template v-if='combinationDirect.length>0'>
                                                <tr v-for="(item,index) in combinationDirect" :key="index">
                                                    <td class="center" v-text="item.productId"></td>
                                                    <td class="center" v-text="item.productName"></td>
                                                    <td class="center" v-text="numFormat(item.totalAmt)"></td>
                                                    <td class="center" v-text="numFormat(item.balanceQuty)"></td>
                                                    <td class="center" v-text="item.nav+' ('+(item.navDate?item.navDate:'--')+')'"></td>
                                                    <td class="center" v-text="numFormat(item.buyOnWayAmt)"></td>
                                                    <td class="center" v-text="numFormat(item.sellOnWayQuty)"></td>
                                                    <td class="center" v-text="numFormat(item.rechargeOnWayAmt)"></td>
                                                    <td class="center" v-text="numFormat(item.melonOnWayAmt)"></td>
                                                    <td class="center" v-text="numFormat(item.availableQuty)"></td>
                                                    <td class="center" v-text="numFormat(item.balanceCost)"></td>
                                                    <td class="center" v-text="numFormat(item.balanceProfit)"></td>
                                                    <td class="center" v-text="numFormat(item.balanceProfitRate)"></td>
                                                    <td class="center" v-text="numFormat(item.totalCost)"></td>
                                                    <td class="center" v-text="numFormat(item.totalProfit)"></td>
                                                    <td class="center" v-text="numFormat(item.lastProfit)"></td>
                                                    <td class="center" v-text="numFormat(item.dayFee)"></td>
                                                </tr>
                                            </template>
                                            <template v-else>
                                                <tr>
                                                    <td class="center" colspan="18">暂无数据</td>
                                                </tr>
                                            </template>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div class="text-center" style="clear: both">
                                    <button type="button" class="btn btn-sm btn-primary" data-dismiss="modal">取消
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- DIALOG ENDS -->
                <div class="form-inline" style="padding: 10px 0;">
                    <!--<div class="form-group">-->
                    <!--<label class="control-label">证件类型:</label>-->
                    <!--<input type="text">-->
                    <!--</div>-->
                    <div class="form-group">
                        <label class="control-label">证件号:</label>
                        <input type="text" v-model="idNo">
                    </div>
                    <div class="form-group">
                        <label class="control-label">客户号:</label>
                        <input type="text" style="width: 120px;" v-model="custNo">
