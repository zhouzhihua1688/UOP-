                            <option value="">请选择</option>
                            <option value="156">156-人民币</option>
                            <option value="840">840-美元</option>
                        </select>
                        <span class="red">*必填&nbsp&nbsp</span>
                    </div>
                    <div class="form-group">
                        <label class="control-label">资产类型:</label>
                        <select name="status" class="form-control" style="width: 100px;" v-model="type" @change="showDay">
                            <option value="">请选择</option>
                            <option value="1">1-切片</option>
                            <option value="2">2-实时</option>
                        </select>
                        <span class="red">*必填&nbsp&nbsp</span>
