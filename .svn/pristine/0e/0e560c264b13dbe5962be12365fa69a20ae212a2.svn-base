<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../../common/head.html %>
    <!-- inline styles related to this page -->
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-datetimepicker.min.css"/>
    <link rel="stylesheet" href="/css/chosen.min.css">
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-multiselect.min.css"/>
    <style>
        .table > tbody > tr > td {
            vertical-align: middle;
        }
        .chosen-single {
            height: 30px !important;
            line-height: 30px !important;
            border-radius: 0 !important;
        }
    </style>
</head>
<body class="no-skin">
<% include ../../common/header.html %>
<div class="main-container ace-save-state" id="main-container">
    <% include ../../common/sidebar.html %>
    <% include ../../common/left-menu.html %>
    <div class="main-content">
        <div class="main-content-inner">
            <div class="page-content">
                <% include ../../common/breadcrumb.html %>
                <% include ../../common/ace-settings-container.html %>
                <div class="row" id="content">
                    <div class="col-xs-12">
                        <!-- 提示信息弹窗 -->
                        <div class="modal" id="info" tabindex="-1" role="dialog" aria-hidden="true"
                             data-backdrop="static">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header" style="padding: 10px;">
                                        <button type="button" class="close" data-dismiss="modal"
                                                aria-hidden="true">×
                                        </button>
                                        <h4 class="modal-title">提示信息</h4>
                                    </div>
                                    <div class="modal-body text-center">
                                        <h4 v-text="diaMsg"></h4>
                                    </div>
                                    <p class="text-center">
                                        <button type="button" class="btn btn-primary btn-sm" data-dismiss="modal">确定
                                        </button>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <!-- 本地新增弹窗 -->
                        <div class="modal" id="addLocal" tabindex="-1" role="dialog" aria-hidden="true"
                             data-backdrop="static">
                            <div class="modal-dialog" style="width: 700px;">
                                <div class="modal-content">
                                    <div class="modal-header" style="padding: 10px;">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                            &times;
                                        </button>
                                        <h4 class="modal-title inline">数据操作</h4>
                                    </div>
                                    <div class="modal-body">
                                        <div class="form">
                                            <div style="float:left;padding-left: 30px;">
                                                <div class="form-group">
                                                    <label class="control-label">产品代码:</label>
                                                    <!--<select id="fundList_local_dialog"-->
                                                            <!--class="chosen-select form-control"-->
                                                            <!--data-placeholder="请选择">-->
                                                    <!--</select>-->
                                                    <select id="fundList" class="multiselect" multiple="multiple">
                                                    </select>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">客户类型:</label>
                                                    <label class="pos-rel">
                                                        <input type="checkbox" id="custType9"
                                                               value="*" class="ace"
                                                               @click="setCustType('*')"
                                                               v-model="local.dialog_custType"/>
                                                        <span class="lbl"></span>
                                                    </label>
                                                    <label for="custType9">全部</label>
                                                    <template v-for="(item,index) in local.dialog_custTypeList">
                                                        <label class="pos-rel">
                                                            <input type="checkbox"
                                                                   :id="'custType'+index"
                                                                   :value="item.key" class="ace"
                                                                   @click="setCustType(item.key)"
                                                                   v-model="local.dialog_custType"/>
                                                            <span class="lbl"></span>
                                                        </label>
                                                        <label :for="'custType'+index"
                                                               v-text="item.value"></label>
                                                    </template>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">交易类型:</label>
                                                    <select style="width:170px;" v-model="local.dialog_tradeType">
                                                        <option :value="item.key"
                                                                v-for="(item,index) in local.dialog_tradeTypeList"
                                                                v-text="item.value">
                                                        </option>
                                                    </select>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">网点:</label>
                                                    <select style="width:170px;" v-model="local.dialog_branchCode">
                                                        <option value="*">全部</option>
                                                        <option :value="item.key"
                                                                v-for="(item,index) in local.dialog_branchCodeList"
                                                                v-text="item.value">
                                                        </option>
                                                    </select>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">渠道:</label>
                                                    <select style="width:170px;" v-model="local.dialog_channel">
                                                        <option value="*">全部</option>
                                                        <option :value="item.key"
                                                                v-for="(item,index) in local.dialog_channelList"
                                                                v-text="item.value">
                                                        </option>
                                                    </select>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">银行编号:</label>
                                                    <input type="text" v-model.trim="local.dialog_bankNo"
                                                           placeholder="可空"/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">目标产品:</label>
                                                    <input type="text" v-model.trim="local.dialog_oproduct"
                                                           placeholder="可空"/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width:60px;">优先级:</label>
                                                    <input type="text" v-model.trim="priority"
                                                           placeholder="数字默认为0"/>
                                                </div>
                                            </div>
                                            <div style="float:right;padding-right: 30px;">
                                                <div class="form-group" style="margin-bottom: 2px;">
                                                    <label class="control-label">交易折扣:</label>
                                                    <input type="text" v-model.trim="local.dialog_tradeDiscount"
                                                           placeholder="必填"
                                                           @input="changeDisplayDiscount('local','dialog')"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <span class="red" style="padding-left: 60px;">取值为0到1,例:5折应填0.5</span>
                                                <div class="form-group" style="margin-bottom: 2px;">
                                                    <label class="control-label">展示折扣:</label>
                                                    <input type="text" v-model.trim="local.dialog_displayDiscount"
                                                           placeholder="必填"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <span class="red" style="padding-left: 60px;">取值为0到1,例:5折应填0.5</span>
                                                <div class="form-group" style="margin-bottom: 5px;">
                                                    <div class="input-group" style="margin-bottom: 5px;">
                                                        <label class="control-label">开始时间:</label>
                                                        <input type="text" id="startTime_local" class="date-timepicker">
                                                        <span class="input-group-addon" style="width: 16%;">
                                                            <i class="fa fa-clock-o bigger-110"></i>
                                                        </span>
                                                    </div>
                                                    <span style="padding-left: 60px;padding-top:10px;">
                                                        <a href="javascript:;"
                                                           @click="setStartTime('now','local')">现在日期</a>&nbsp;
                                                        <a href="javascript:;" @click="setStartTime('future','local')">未来日期</a>&nbsp;
                                                        <a href="javascript:;" @click="setStartTime(0,'local')">0点</a>&nbsp;
                                                        <a href="javascript:;" @click="setStartTime(9,'local')">9点</a>&nbsp;
                                                        <a href="javascript:;" @click="setStartTime(15,'local')">15点</a>
                                                    </span>
                                                </div>
                                                <div class="form-group" style="margin-bottom: 5px;">
                                                    <div class="input-group" style="margin-bottom: 5px;">
                                                        <label class="control-label">结束时间:</label>
                                                        <input type="text" id="endTime_local" class="date-timepicker">
                                                        <span class="input-group-addon" style="width: 16%;">
                                                            <i class="fa fa-clock-o bigger-110"></i>
                                                        </span>
                                                    </div>
                                                    <span style="padding-left: 60px;">
                                                        <a href="javascript:;"
                                                           @click="setEndTime('now','local')">现在日期</a>&nbsp;
                                                        <a href="javascript:;" @click="setEndTime('future','local')">未来日期</a>&nbsp;
                                                        <a href="javascript:;" @click="setEndTime(0,'local')">0点</a>&nbsp;
                                                        <a href="javascript:;" @click="setEndTime(9,'local')">9点</a>&nbsp;
                                                        <a href="javascript:;" @click="setEndTime(15,'local')">15点</a>
                                                    </span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">起始金额:</label>
                                                    <input type="text" v-model.trim="local.dialog_startAmount"
                                                           placeholder="必填"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">结束金额:</label>
                                                    <input type="text" v-model.trim="local.dialog_endAmount"
                                                           placeholder="必填"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">备注:</label>
                                                    <input type="text" v-model.trim="local.dialog_remark"
                                                           placeholder="可空"/>
                                                </div>
                                            </div>
                                            <div style="clear: both"></div>
                                            <p class="text-center">
                                                <button class="btn btn-sm btn-primary" @click="addLocal">确定
                                                </button>
                                                <button class="btn btn-sm btn-primary" data-dismiss="modal"
                                                        aria-hidden="true">取消
                                                </button>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- 本地查看和修改弹窗 -->
                        <div class="modal" id="updateLocal" tabindex="-1" role="dialog" aria-hidden="true"
                             data-backdrop="static">
                            <div class="modal-dialog" style="width: 700px;">
                                <div class="modal-content">
                                    <div class="modal-header" style="padding: 10px;">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                            &times;
                                        </button>
                                        <h4 class="modal-title inline">数据操作</h4>
                                    </div>
                                    <div class="modal-body">
                                        <div class="form">
                                            <div style="float:left;padding-left: 30px;">
                                                <div class="form-group">
                                                    <label class="control-label">产品代码:</label>
                                                    <input type="text" :value="local.dialog_product"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">客户类型:</label>
                                                    <input type="text"
                                                           :value="printCustType(local.dialog_custType)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">交易类型:</label>
                                                    <input type="text" :value="printTradeType(local.dialog_tradeType)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">网点:</label>
                                                    <input type="text" :value="printBranchCode(local.dialog_branchCode)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">渠道:</label>
                                                    <input type="text" :value="printChannel(local.dialog_channel)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">银行编号:</label>
                                                    <input type="text" :value="local.dialog_bankNo"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">目标产品:</label>
                                                    <input type="text" :value="local.dialog_oproduct"
                                                           disabled/>
                                                </div>
                                            </div>
                                            <div style="float:right;padding-right: 30px;">
                                                <div class="form-group" style="margin-bottom: 2px;">
                                                    <label class="control-label">交易折扣:</label>
                                                    <input type="text" v-model.trim="local.dialog_tradeDiscount"
                                                           placeholder="必填" :disabled="local.dialog_isCheck"
                                                           @input="changeDisplayDiscount('local','dialog')"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <span class="red" style="padding-left: 60px;">取值为0到1,例:5折应填0.5</span>
                                                <div class="form-group" style="margin-bottom: 2px;">
                                                    <label class="control-label">展示折扣:</label>
                                                    <input type="text" v-model.trim="local.dialog_displayDiscount"
                                                           placeholder="必填" :disabled="local.dialog_isCheck"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <span class="red" style="padding-left: 60px;">取值为0到1,例:5折应填0.5</span>
                                                <div class="form-group" style="margin-bottom: 5px;">
                                                    <div class="input-group" style="margin-bottom: 5px;">
                                                        <label class="control-label">开始时间:</label>
                                                        <input type="text" id="startTime_local_update"
                                                               class="date-timepicker"
                                                               :disabled="local.dialog_isCheck">
                                                        <span class="input-group-addon" style="width: 16%;">
                                                            <i class="fa fa-clock-o bigger-110"></i>
                                                        </span>
                                                    </div>
                                                    <span style="padding-left: 60px;" v-show="local.dialog_isUpdate">
                                                        <a href="javascript:;"
                                                           @click="setStartTime('now','local_update')">现在日期</a>&nbsp;
                                                        <a href="javascript:;"
                                                           @click="setStartTime('future','local_update')">未来日期</a>&nbsp;
                                                        <a href="javascript:;" @click="setStartTime(0,'local_update')">0点</a>&nbsp;
                                                        <a href="javascript:;" @click="setStartTime(9,'local_update')">9点</a>&nbsp;
                                                        <a href="javascript:;" @click="setStartTime(15,'local_update')">15点</a>
                                                    </span>
                                                </div>
                                                <div class="form-group" style="margin-bottom: 5px;">
                                                    <div class="input-group" style="margin-bottom: 5px;">
                                                        <label class="control-label">结束时间:</label>
                                                        <input type="text" id="endTime_local_update"
                                                               class="date-timepicker"
                                                               :disabled="local.dialog_isCheck">
                                                        <span class="input-group-addon" style="width: 16%;">
                                                            <i class="fa fa-clock-o bigger-110"></i>
                                                        </span>
                                                    </div>
                                                    <span style="padding-left: 60px;" v-show="local.dialog_isUpdate">
                                                        <a href="javascript:;"
                                                           @click="setEndTime('now','local_update')">现在日期</a>&nbsp;
                                                        <a href="javascript:;"
                                                           @click="setEndTime('future','local_update')">未来日期</a>&nbsp;
                                                        <a href="javascript:;"
                                                           @click="setEndTime(0,'local_update')">0点</a>&nbsp;
                                                        <a href="javascript:;"
                                                           @click="setEndTime(9,'local_update')">9点</a>&nbsp;
                                                        <a href="javascript:;" @click="setEndTime(15,'local_update')">15点</a>
                                                    </span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">起始金额:</label>
                                                    <input type="text" v-model.trim="local.dialog_startAmount"
                                                           placeholder="必填" :disabled="local.dialog_isCheck"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">结束金额:</label>
                                                    <input type="text" v-model.trim="local.dialog_endAmount"
                                                           placeholder="必填" :disabled="local.dialog_isCheck"/>
                                                    <span style="color:red">*必填</span>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">备注:</label>
                                                    <input type="text" v-model.trim="local.dialog_remark"
                                                           :disabled="local.dialog_isCheck"/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width:60px;">优先级:</label>
                                                    <input type="text" v-model.trim="priority" :disabled="local.dialog_isCheck"/>
                                                </div>
                                            </div>
                                            <div style="clear: both"></div>
                                            <template v-if="!local.dialog_isCheck">
                                                <p class="text-center">
                                                    <button class="btn btn-sm btn-primary" @click="updateLocal">确定
                                                    </button>
                                                    <button class="btn btn-sm btn-primary" data-dismiss="modal"
                                                            aria-hidden="true">取消
                                                    </button>
                                                </p>
                                            </template>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- 撤销弹窗 -->
                        <div class="modal" id="localRevert" tabindex="-1" role="dialog" aria-hidden="true"
                             data-backdrop="static">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header" style="padding: 10px;">
                                        <button type="button" class="close" data-dismiss="modal"
                                                aria-hidden="true">×
                                        </button>
                                        <h4 class="modal-title">撤销</h4>
                                    </div>
                                    <div class="modal-body text-center">
                                        <h4>确定撤销该数据？</h4>
                                    </div>
                                    <p class="text-center">
                                        <button type="button" class="btn btn-primary btn-sm" @click="revert">确定
                                        </button>
                                        <button class="btn btn-sm btn-primary" data-dismiss="modal"
                                                aria-hidden="true">取消
                                        </button>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <!-- 提交弹窗 -->
                        <div class="modal" id="localSubmit" tabindex="-1" role="dialog" aria-hidden="true"
                             data-backdrop="static">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header" style="padding: 10px;">
                                        <button type="button" class="close" data-dismiss="modal"
                                                aria-hidden="true">×
                                        </button>
                                        <h4 class="modal-title">提交</h4>
                                    </div>
                                    <div class="modal-body text-center">
                                        <h4>确定提交该数据？(提交后不可更改)</h4>
                                    </div>
                                    <p class="text-center">
                                        <button type="button" class="btn btn-primary btn-sm">确定
                                        </button>
                                        <button class="btn btn-sm btn-primary" data-dismiss="modal"
                                                aria-hidden="true">取消
                                        </button>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <!-- 重新提交删除弹窗 -->
                        <div class="modal" id="submitAgainDelete" tabindex="-1" role="dialog" aria-hidden="true"
                             data-backdrop="static">
                            <div class="modal-dialog">
                                <div class="modal-content">
                                    <div class="modal-header" style="padding: 10px;">
                                        <button type="button" class="close" data-dismiss="modal"
                                                aria-hidden="true">×
                                        </button>
                                        <h4 class="modal-title">删除</h4>
                                    </div>
                                    <div class="modal-body text-center">
                                        <h4>确定重新提交删除操作？</h4>
                                    </div>
                                    <p class="text-center">
                                        <button type="button" class="btn btn-primary btn-sm"
                                                @click="submitAgain">确定
                                        </button>
                                        <button class="btn btn-sm btn-primary" data-dismiss="modal"
                                                aria-hidden="true">取消
                                        </button>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <!-- 重新提交新增和修改弹窗 -->
                        <div class="modal" id="submitAgainOperate" tabindex="-1" role="dialog" aria-hidden="true"
                             data-backdrop="static">
                            <div class="modal-dialog" style="width: 700px;">
                                <div class="modal-content">
                                    <div class="modal-header" style="padding: 10px;">
                                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                                            &times;
                                        </button>
                                        <h4 class="modal-title inline">数据操作</h4>
                                    </div>
                                    <div class="modal-body">
                                        <div class="form">
                                            <div style="float:left;padding-left: 30px;">
                                                <div class="form-group">
                                                    <label class="control-label">产品代码:</label>
                                                    <input type="text" :value="local.submitAgain_product" disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">客户类型:</label>
                                                    <input type="text"
                                                           :value="printCustType(local.submitAgain_custType)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">交易类型:</label>
                                                    <input type="text"
                                                           :value="printTradeType(local.submitAgain_tradeType)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">网点:</label>
                                                    <input type="text"
                                                           :value="printBranchCode(local.submitAgain_branchCode)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label" style="width: 60px;">渠道:</label>
                                                    <input type="text" :value="printChannel(local.submitAgain_channel)"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">银行编号:</label>
                                                    <input type="text" :value="local.submitAgain_bankNo"
                                                           disabled/>
                                                </div>
                                                <div class="form-group">
                                                    <label class="control-label">目标产品:</label>
                                                    <input type="text" :value="local.submitAgain_oproduct"
                                                           disabled/>
                                                </div>
                                            </div>
                                            <div style="float:right;padding-right: 30px;">
                                                <div class="form-group" style="margin-bottom: 2px;">
                                                    <label class="control-label">交易折扣:</label>
                                                    <input type="text" v-model.trim="local.submitAgain_tradeDiscount"
                                                           placeholder="必填"
                                                           @input="changeDisplayDiscount('local','submitAgain')"/>
                                                </div>
                                                <span class="red" style="padding-left: 60px;">取值为0到1,例:5折应填0.5</span>
                                                <div class="form-group" style="margin-bottom: 2px;">
                                                    <label class="control-label">展示折扣:</label>
                                                    <input type="text" v-model.trim="local.submitAgain_displayDiscount"
                                                           placeholder="必填"/>
                                                </div>
                                                <span class="red" style="padding-left: 60px;">取值为0到1,例:5折应填0.5</span>
                                                <div class="form-group" style="margin-bottom: 10px;">
                                                    <div class="input-group" style="margin-bottom: 10px;">
                                                        <label class="control-label">开始时间:</label>
                                                        <input type="text" id="startTime_submit"
                                                               class="date-timepicker">
                                                        <span class="input-group-addon" style="width: 16%;">
                                                        <i class="fa fa-clock-o bigger-110"></i>
                                                    </span>
                                                    </div>
