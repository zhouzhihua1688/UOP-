<div class="channel">
    <input type="button" class="btn-primary" id="prevChannel" value="<">
    <div class="channel-nav">
        <ul class="channel-sidebar">
            <% channels.forEach(function(item) {%>
            <% if (item.checked) { %>
            <li class="selected">
                <% } else { %>
            <li>
                <% } %>
                <a href="<%= item.url + '/index.html' %>">
                    <span><%= item.name  %></span>
                </a>
            </li>
            <% }) %>
        </ul>
    </div>
    <input type="button" class="btn-primary" id="nextChannel" value=">">
</div>


