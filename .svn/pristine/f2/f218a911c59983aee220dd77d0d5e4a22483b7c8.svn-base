    <!-- inline styles related to this page -->
    <link rel="stylesheet" href="/ace/assets/css/bootstrap-datetimepicker.min.css"/>
    <style>
        .table > tbody > tr > td {
            vertical-align: middle;
        }
        /* .date-timepicker{
            position: absolute;
            left: 0;
            top: 0;
        } */
        .clearfix:after {
            content:""; 
            display: block; 
            clear:both; 
        }
    </style>
</head>
<body class="no-skin">
<% include ../../common/header.html %>
