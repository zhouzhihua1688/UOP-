                            <label class="pos-rel">
                                <input type="checkbox" :id="index" :value="item.thmconfigId" class="ace"
                                      v-model="thmconfigId" />
                                <span class="lbl"></span>
                            </label>
                            <label :for="index" v-text="item.layoutString"></label>
                        </div>
                    </template>
                    <template v-else>
                        <template v-if="hasthemeInfoData"><p class="text-center" style="margin: 0;">暂无数据</p></template>   
                        <template v-else><p class="text-center" style="margin: 0;">正在加载主题...</p></template>     
                    </template>
                </div>
